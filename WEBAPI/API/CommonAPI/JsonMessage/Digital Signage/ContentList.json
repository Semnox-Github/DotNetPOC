    "DynamicFlag": "Y",
    "Query": "\nselect top 1\n           c.card_number,\nRIGHT('0000' + CONVERT(VARCHAR(5),sum(quantity)), 4) Qty, RIGHT('0000' + CONVERT(varchar(5),sum(balancegames)), 4)\n   from CardGames cg,cards c\nwhere c.card_id in (\n                                 select top 1 card_id\n                                    from gameplay\n                                  where machine_id in (select machine_id\n                                                                              from machines\n                                                                           where machine_name = 'CarRace2')\n                                   and play_date > dateadd(ss,-19,GETDATE())\n                                     order by play_date desc)\nand c.card_id = cg.card_id\ngroup by c.card_number",
