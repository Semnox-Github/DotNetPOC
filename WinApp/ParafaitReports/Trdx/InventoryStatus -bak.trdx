    <SqlDataSource QueryDesignerState="null" ConnectionString="ParafaitReportsNew" SelectCommand="IF (@isCorporate = 'Y')&#xD;&#xA;BEGIN&#xD;&#xA;&#x9;IF(@mode = 'W' or @mode = 'S')&#xD;&#xA;&#x9;BEGIN&#xD;&#xA;&#x9;&#x9;select FunctionId as functionId from managementformaccess where FunctionGroup = 'Data Access' &#xD;&#xA;&#x9;&#x9;&#x9;and access_allowed = 'Y' and main_menu = 'Sites'&#xD;&#xA;                        and (role_id = @role or -1 = @role)&#xD;&#xA;&#x9;END&#xD;&#xA;&#x9;ELSE&#xD;&#xA;&#x9;BEGIN&#xD;&#xA;&#x9;&#x9;select FunctionId as functionId from managementformaccess where FunctionGroup = 'Data Access' &#xD;&#xA;&#x9;&#x9;&#x9;and access_allowed = 'Y' and main_menu = 'Sites'&#xD;&#xA;&#x9;&#x9;&#x9;and (role_id = @role or -1 = @role)&#xD;&#xA;&#x9;&#x9;&#x9;and FunctionId = (select site_id from user_roles where role_id = @role)&#xD;&#xA;&#x9;END&#xD;&#xA;END&#xD;&#xA;ELSE&#xD;&#xA;BEGIN&#xD;&#xA;&#x9;SELECT NULL as functionId&#xD;&#xA;END&#xD;&#xA;" CommandTimeout="600" Name="dsFunctionId">
