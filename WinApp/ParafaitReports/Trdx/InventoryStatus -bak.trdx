    <SqlDataSource QueryDesignerState="null" ConnectionString="ParafaitReportsNew" SelectCommand="select location Location, Product, Description, IsRedeemable, IsSellable, IsActive&#xD;&#xA;    , sum(Opening_Balance) Open_Qty, sum(Opening_Balance * Rate) / case sum(Opening_Balance) when 0 then 1 else sum(Opening_Balance) end Open_Rate, sum(Opening_Balance * Rate) Open_Value &#xD;&#xA;    , sum(Received) PO_Qty, sum(Received * PurchasePrice) / case sum(Received) when 0 then 1 else sum(Received) end PO_Rate, sum(Received * PurchasePrice) PO_Value &#xD;&#xA;    , sum(Sold) Sale_Qty, sum(Sold * SalePrice) / case sum(Sold) when 0 then 1 else sum(Sold) end Sale_Rate, sum(Sold * SalePrice * -1) sale_Value &#xD;&#xA;    , sum(Redeemed) redeem_qty, sum(Redeemed * Rate) / case sum(Redeemed) when 0 then 1 else sum(Redeemed) end redeem_rate, sum(Redeemed * Rate) redeem_Value &#xD;&#xA;    , sum(Transfers) tr_qty, sum(Transfers * Rate) / case sum(Transfers) when 0 then 1 else sum(Transfers) end tr_rate, sum(Transfers * Rate) transfer_value &#xD;&#xA;    , sum(Adjustments) adj_qty, sum(Adjustments * Rate) / case sum(Adjustments) when 0 then 1 else sum(Adjustments) end adj_rate, sum(Adjustments * Rate) adj_value &#xD;&#xA;    , sum(On_Hand) Closing_Qty, sum(On_Hand * Rate) / case sum(On_Hand) when 0 then 1 else sum(On_Hand) end closing_rate, sum(On_Hand * Rate) Closing_value &#xD;&#xA;from ( &#xD;&#xA;select p.code Product, p.Description, p.isSellable, p.isRedeemable, p.isActive, &#xD;&#xA;    l.name Location, l.LocationId, p.Cost Rate, &#xD;&#xA;    (i.quantity +  &#xD;&#xA;    isnull(-1 * my_receipts.quantity_open, 0) +  &#xD;&#xA;    isnull(-1 * my_adj.quantity_open, 0) +  &#xD;&#xA;    isnull(-1 * my_transfer.quantity_open, 0) +  &#xD;&#xA;    isnull(-1 * sold.quantity_open, 0) +  &#xD;&#xA;    isnull(-1 * my_redemption.quantity_open, 0)) Opening_Balance, &#xD;&#xA;    isnull(my_receipts.Quantity, 0) Received, &#xD;&#xA;    isnull(my_redemption.quantity, 0) Redeemed, &#xD;&#xA;    isnull(sold.quantity, 0) Sold, &#xD;&#xA;    isnull(sold.SalePrice, 0) SalePrice, &#xD;&#xA;    isnull(my_receipts.PurchasePrice, 0) PurchasePrice, &#xD;&#xA;    isnull(my_adj.quantity, 0) Adjustments, &#xD;&#xA;    isnull(my_transfer.quantity, 0) Transfers, &#xD;&#xA;(i.quantity +  &#xD;&#xA;    isnull(-1 * my_receipts.quantity_open, 0) +  &#xD;&#xA;    isnull(-1 * my_adj.quantity_open, 0) +  &#xD;&#xA;    isnull(-1 * my_transfer.quantity_open, 0) +  &#xD;&#xA;    isnull(-1 * sold.quantity_open, 0) +  &#xD;&#xA;    isnull(-1 * my_redemption.quantity_open, 0) +&#xD;&#xA;    isnull(my_receipts.quantity, 0) +  &#xD;&#xA;    isnull(my_adj.quantity, 0) +  &#xD;&#xA;    isnull(my_transfer.quantity, 0) +  &#xD;&#xA;    isnull(sold.quantity, 0) +  &#xD;&#xA;    isnull(my_redemption.quantity, 0)) On_Hand,&#xD;&#xA;    l.IsAvailableToSell &#xD;&#xA;from inventory i  &#xD;&#xA;&#xD;&#xA;/* starting join for the Received numbers */&#xD;&#xA;left outer join (select rl.productid, locationid, sum(case when ReceiveDate &lt;= dbo.ConvertToServerTime(@toDate,@offSet) then isnull(quantity,0) else 0 end) quantity, &#xD;&#xA;                        sum(isnull(quantity,0)) quantity_open, PurchasePrice  &#xD;&#xA;    from PurchaseOrderReceive_Line rl left outer join (select l.productId, sum(l.amount)/sum(l.quantity) PurchasePrice &#xD;&#xA;        from PurchaseOrderReceive_Line l, Receipt r &#xD;&#xA;        where ReceiveDate between dbo.ConvertToServerTime(@fromDate,@offSet) and dbo.ConvertToServerTime(@toDate,@offSet) &#xD;&#xA;        and isreceived = 'Y' &#xD;&#xA;        and r.ReceiptId = l.ReceiptId &#xD;&#xA;        group by l.productId) v on rl.productId = v.productId , Receipt r &#xD;&#xA;where isreceived = 'Y' and rl.ReceiptId = r.ReceiptId &#xD;&#xA;and ((r.site_id in (@site)) or (-1 in (@site)))&#xD;&#xA;and ReceiveDate &gt;= dbo.ConvertToServerTime(@fromDate,@offSet) &#xD;&#xA;group by rl.productid, locationid, PurchasePrice) my_receipts &#xD;&#xA;on my_receipts.productid = i.productid and &#xD;&#xA;my_receipts.locationid = i.Locationid &#xD;&#xA;&#xD;&#xA;/* end for Received numbers and starting adjustments */&#xD;&#xA;left outer join (select productid, fromlocationID location, &#xD;&#xA;sum(case when timestamp &lt;= dbo.ConvertToServerTime(@toDate,@offSet) then isnull(adjustmentquantity, 0) else 0 end) quantity, &#xD;&#xA;sum(isnull(adjustmentquantity, 0)) quantity_open &#xD;&#xA;from inventoryadjustments &#xD;&#xA;where adjustmenttype in ('Adjustment', 'Trade-In') &#xD;&#xA;and timestamp &gt;= dbo.ConvertToServerTime(@fromDate,@offSet) &#xD;&#xA;and ((site_id in (@site)) or (-1 in (@site)))&#xD;&#xA;group by productid,fromlocationID) my_adj &#xD;&#xA;on my_adj.productid = i.productid and &#xD;&#xA;    my_adj.location = i.Locationid &#xD;&#xA;&#xD;&#xA;left outer join (select productId, location, sum(quantity) quantity, sum(quantity_open) quantity_open&#xD;&#xA;    from (select productid, tolocationID location,&#xD;&#xA;                sum(case when timestamp &lt;= dbo.ConvertToServerTime(@toDate,@offSet) then isnull(adjustmentquantity, 0) else 0 end) quantity, &#xD;&#xA;                sum(isnull(adjustmentquantity, 0)) quantity_open&#xD;&#xA;    from inventoryadjustments &#xD;&#xA;    where adjustmenttype = 'Transfer' &#xD;&#xA;    and timestamp &gt;= dbo.ConvertToServerTime(@fromDate,@offSet)&#xD;&#xA;&#x9;and ((site_id in (@site)) or (-1 in (@site)))&#xD;&#xA;    group by productid,tolocationID  &#xD;&#xA;        union all &#xD;&#xA;        select productid, fromlocationID location, &#xD;&#xA;    sum(case when timestamp &lt;= dbo.ConvertToServerTime(@toDate,@offSet) then isnull(adjustmentquantity, 0)*-1 else 0 end) quantity, &#xD;&#xA;    sum(isnull(adjustmentquantity, 0))*-1 quantity_open &#xD;&#xA;    from inventoryadjustments &#xD;&#xA;    where adjustmenttype = 'Transfer' &#xD;&#xA;    and timestamp &gt;= dbo.ConvertToServerTime(@fromDate,@offSet) &#xD;&#xA;&#x9;and ((site_id in (@site)) or (-1 in (@site)))&#xD;&#xA;    group by productid, fromlocationID ) vv  &#xD;&#xA;    group by productId, location ) my_transfer &#xD;&#xA;on my_transfer.productid = i.productid and &#xD;&#xA;    my_transfer.location = i.Locationid &#xD;&#xA;&#xD;&#xA;/* end for adjustments and starting redemption*/&#xD;&#xA;left outer join (select pp.productid, OutboundLocationid,&#xD;&#xA;                        sum(case when redeemed_date &lt;= dbo.ConvertToServerTime(@toDate,@offSet) then -1 else 0 end) quantity, &#xD;&#xA;                        count(pp.productid)*-1 quantity_open &#xD;&#xA;                from redemption_gifts rg, redemption r, product pp &#xD;&#xA;                where rg.redemption_id = r.redemption_id and &#xD;&#xA;                pp.code = rg.gift_code &#xD;&#xA;                and redeemed_date &gt;= dbo.ConvertToServerTime(@fromDate,@offSet) &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;and ((r.site_id in (@site)) or (-1 in (@site)))&#xD;&#xA;                group by pp.productid, OutboundLocationid) my_redemption &#xD;&#xA;on my_redemption.productid = i.productid and &#xD;&#xA;    my_redemption.OutboundLocationid = i.Locationid &#xD;&#xA;                /* end for  redemption, starting sales*/&#xD;&#xA;&#xD;&#xA;left outer join (select productid, Locationid, &#xD;&#xA;sum(case when TrxDate &lt;= dbo.ConvertToServerTime(@toDate,@offSet) then (SalePrice * (1 + isnull(TaxPercentage, 0)/100) * quantity) else 0 end) / sum(case when TrxDate &lt;= dbo.ConvertToServerTime(@toDate,@offSet) then isnull(quantity, 0) else null end) SalePrice,  &#xD;&#xA;sum(case when TrxDate &lt;= dbo.ConvertToServerTime(@toDate,@offSet) then isnull(quantity,0)*-1 else 0 end) quantity, &#xD;&#xA;sum(isnull(quantity,0))*-1 quantity_open&#xD;&#xA;from InventoryTransaction r &#xD;&#xA;where TrxDate &gt;= dbo.ConvertToServerTime(@fromDate,@offSet) &#xD;&#xA;and ((site_id in (@site)) or (-1 in (@site)))&#xD;&#xA;group by productid, Locationid) sold &#xD;&#xA;on sold.productid = i.productid and &#xD;&#xA;    sold.Locationid = i.Locationid &#xD;&#xA;                /* end for sales */&#xD;&#xA;&#xD;&#xA;,product p, location l &#xD;&#xA;where p.productid = i.productid &#xD;&#xA;and ((i.site_id in (@site)) or (-1 in (@site)))&#xD;&#xA;and l.locationid =  i.locationid)  mainQuery &#xD;&#xA;    where 1 = 1 &#xD;&#xA;    and LocationId in (@locationId)&#xD;&#xA;    group by Location,Product,Description, IsRedeemable, IsSellable, IsActive&#xD;&#xA;order by 1, 2&#xD;&#xA;" CommandTimeout="600" Name="dsInventory">
