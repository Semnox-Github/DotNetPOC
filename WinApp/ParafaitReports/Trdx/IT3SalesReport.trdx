    <SqlDataSource QueryDesignerState="null" ConnectionString="parafaitOld" SelectCommand="exec dbo.SetContextInfo @loginUserId ;&#xD;&#xA;&#xD;&#xA;select case when sv1.ValueChar = 'Brand Partner' then 'BP - ' + sv4.ValueChar  &#xD;&#xA;            WHEN ISNULL(sv4.ValueChar,'Others') like 'MRP%' then sv1.ValueChar + ' ' + sv4.ValueChar &#xD;&#xA;&#x9;&#x9;&#x9;ELSE sv1.ValueChar + ' Food' END Collection,&#xD;&#xA;           sum(quantity) Qty, &#xD;&#xA;&#x9;   sum(net_amount * GameCardRatio) Card_Amount, &#xD;&#xA;&#x9;   sum(net_amount * (CashRatio + CreditCardRatio)) Net_Total, &#xD;&#xA;&#x9;   sum(tax) Tax, &#xD;&#xA; &#x9;   sum(ISNULL(amount,0)) Total,  &#xD;&#xA;           CASE when sv1.ValueChar = 'Brand Partner' then &#xD;&#xA;                ISNULL(l.Revenue_Share,0) &#xD;&#xA;&#x9;   ELSE '100.00' END &quot;NHPL Revenue%&quot;,&#xD;&#xA;           SUM(CASE when sv1.ValueChar = 'Brand Partner' then &#xD;&#xA;                ISNULL(amount,0)/(1+ISNULL(l.Tax_Pct,0)/100) *  ISNULL(l.Revenue_Share,0)/100&#xD;&#xA;           ELSE &#xD;&#xA;&#x9;&#x9;ISNULL(net_amount * (GameCardRatio + CashRatio + CreditCardRatio),0) END) NHPL_Revenue&#xD;&#xA;  from TransactionView v,&#xD;&#xA;       product_type ptp,&#xD;&#xA;       products p,&#xD;&#xA;       segmentdataview sv1,&#xD;&#xA;       segmentdataview sv4&#xD;&#xA;       left outer join (select substring(lv.lookupvalue,1,CHARINDEX('|',lv.lookupvalue)-1) Before_tax,&#xD;&#xA;                               CONVERT(NUMERIC,substring(lv.lookupvalue,CHARINDEX('|',lv.lookupvalue)+1,CHARINDEX('|',lv.lookupvalue,CHARINDEX('|',lv.lookupvalue)+1)-3),2) Tax_Pct,&#xD;&#xA;                               CONVERT(NUMERIC,right(lv.lookupvalue,len(lv.lookupvalue)-CHARINDEX('|',lv.lookupvalue,CHARINDEX('|',lv.lookupvalue)+1)),2) Revenue_Share,&#xD;&#xA;                               lv.description from LookupValues lv,lookups l &#xD;&#xA;&#x9;   where l.LookupName = 'IT3_REVENUE_SHARE_PCT' and l.lookupid = lv.lookupid) &#xD;&#xA;&#x9;   l on l.description = sv4.ValueChar &#xD;&#xA;  where trxdate &gt;= @fromDate and trxdate &lt;= @toDate&#xD;&#xA;    and v.product_type = ptp.product_type&#xD;&#xA;    and ptp.cardsale &lt;&gt; 'Y'&#xD;&#xA;    and v.product_id = p.product_id&#xD;&#xA;    and v.pos_counter like 'IT3'&#xD;&#xA;    and p.segmentCategoryId = sv1.segmentCategoryId&#xD;&#xA;    and sv1.SegmentName = 'Product Category'&#xD;&#xA;    and p.segmentCategoryId = sv4.segmentCategoryId&#xD;&#xA;    and sv4.SegmentName = 'Brands'&#xD;&#xA;group by case when sv1.ValueChar = 'Brand Partner' then 'BP - ' + sv4.ValueChar  &#xD;&#xA;            WHEN ISNULL(sv4.ValueChar,'Others') like 'MRP%' then sv1.ValueChar + ' ' + sv4.ValueChar &#xD;&#xA;&#x9;&#x9;&#x9;ELSE sv1.ValueChar + ' Food' END,&#xD;&#xA;         CASE when sv1.ValueChar = 'Brand Partner' then &#xD;&#xA;              ISNULL(l.Revenue_Share,0) &#xD;&#xA;&#x9;   ELSE '100.00' END" CommandTimeout="600" Name="dsRevenue">
