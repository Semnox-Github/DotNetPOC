    <SqlDataSource QueryDesignerState="null" ConnectionString="Data Source=.\sqlexpress;Initial Catalog=Parafait;Integrated Security=True" ProviderName="System.Data.SqlClient" SelectCommand="SELECT tax_name,sum(tax_amount) tax_amount from&#xD;&#xA;(select DISTINCT isnull(structureName, tax_name) tax_name,0 Tax_Amount&#xD;&#xA;  from tax t left outer join TaxStructure ts on ts.TaxId=t.tax_id&#xD;&#xA;UNION ALL&#xD;&#xA;select tax_name, sum(DiscountedTaxAmount) tax_amount&#xD;&#xA;  from trx_Header v , (select l.TrxId, isnull(structureName, tax_name) tax_Name, sum(amount * (1 - isnull(d.discountPercentage, 0))) DiscountedTaxAmount&#xD;&#xA;                    from TrxTaxLines l left outer join TaxStructure ts&#xD;&#xA;                        on ts.TaxStructureId = l.TaxStructureId&#xD;&#xA;                    left outer join&#xD;&#xA;                        (select TrxId, LineId, sum(DiscountPercentage)/100.0 discountPercentage&#xD;&#xA;                                from trxDiscounts&#xD;&#xA;                                group by TrxId, LineId) d&#xD;&#xA;                            on d.TrxId = l.TrxId and d.LineId = l.LineId, Tax t&#xD;&#xA;                         where t.tax_id = l.taxId&#xD;&#xA;&#x9;&#x9;         group by l.TrxId, isnull(structureName, tax_name), d.discountPercentage) inview,&#xD;&#xA;&#x9;  postypes pt&#xD;&#xA; where v.trxid = inview.trxid&#xD;&#xA;   and trxdate between @fromDate and @toDate&#xD;&#xA;   and pt.POSTypeId = v.POSTypeId&#xD;&#xA;   and pt.POSTypeName = 'IT3'&#xD;&#xA;group by tax_name) a&#xD;&#xA;group by tax_name" CommandTimeout="600" Name="dsTaxBreakdown">
