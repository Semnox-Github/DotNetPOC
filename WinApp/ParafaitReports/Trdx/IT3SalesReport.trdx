    <SqlDataSource QueryDesignerState="null" ConnectionString="parafaitOld" SelectCommand="exec dbo.SetContextInfo @loginUserId ;&#xD;&#xA;&#xD;&#xA;select 'Loaded' Type, count(distinct card_id)  Qty, sum(creditplus) Amount&#xD;&#xA;from cardcreditplus&#xD;&#xA;where creationdate  &gt;= @fromDate&#xD;&#xA;            AND creationdate &lt;= dbo.ConvertToServerTime(@toDate,@offSet) &#xD;&#xA;union all&#xD;&#xA;select 'Encashed',count(distinct cardid) , sum(amount)&#xD;&#xA;from trxpayments p, trx_header h&#xD;&#xA;where p.trxid = h.trxid&#xD;&#xA;            and trxdate  &gt;= dbo.ConvertToServerTime(@fromDate,@offSet)&#xD;&#xA;            AND trxdate &lt;= dbo.ConvertToServerTime(@toDate,@offSet) &#xD;&#xA;            and cardcreditplusid is not null&#xD;&#xA;            and cardid is not null&#xD;&#xA;union all&#xD;&#xA;select 'Expired', count(distinct card_id),  sum(CreditPlusBalance)&#xD;&#xA;from cardcreditplus&#xD;&#xA;where periodto &gt;= dbo.ConvertToServerTime(@fromDate,@offSet)&#xD;&#xA;            AND periodto &lt;= dbo.ConvertToServerTime(@toDate,@offSet) &#xD;&#xA;union all&#xD;&#xA;select 'Refunded', sum(case product_type when 'REFUNDCARDDEPOSIT' then quantity else 0 end), sum(amount * (cashratio + creditcardratio))&#xD;&#xA;from transactionview&#xD;&#xA;where trxdate &gt;= dbo.ConvertToServerTime(@fromDate,@offSet)&#xD;&#xA;            AND trxdate &lt;= dbo.ConvertToServerTime(@toDate,@offSet) &#xD;&#xA;            and product_type in ('REFUNDCARDDEPOSIT', 'REFUND')&#xD;&#xA;union all&#xD;&#xA;select 'Valid ', null, null&#xD;&#xA;--select 'Valid ', count(*), sum(creditpluscardbalance + creditpluscredits + creditplusbonus)&#xD;&#xA;--from cardview&#xD;&#xA;--where valid_flag = 'Y' &#xD;&#xA; &#xD;&#xA;union all&#xD;&#xA;select 'Deposit', sum(quantity), sum(amount * (cashratio + creditcardratio))&#xD;&#xA;from transactionview&#xD;&#xA;where trxdate &gt;= dbo.ConvertToServerTime(@fromDate,@offSet)&#xD;&#xA;            AND trxdate &lt;= dbo.ConvertToServerTime(@toDate,@offSet) &#xD;&#xA;            and product_type = 'CARDDEPOSIT'" CommandTimeout="600" Name="dsCardBalance">
