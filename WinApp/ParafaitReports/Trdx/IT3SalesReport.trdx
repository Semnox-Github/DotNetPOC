    <SqlDataSource QueryDesignerState="null" ConnectionString="Data Source=.\sqlexpress;Initial Catalog=Parafait;Integrated Security=True" ProviderName="System.Data.SqlClient" SelectCommand="select discount_name, us.username, count(distinct v.trxId) ops, &#xD;&#xA;        sum(l.DiscountAmount/(case DiscountPercentage when 0 then null else DiscountPercentage end) * 100) original_amount,  &#xD;&#xA;        sum(l.DiscountAmount) discount,  &#xD;&#xA;        sum(l.DiscountAmount/(case DiscountPercentage when 0 then null else DiscountPercentage end) * 100 - l.DiscountAmount) charged &#xD;&#xA;        from (select DiscountId, td.TrxId, l.LineId, sum(DiscountPercentage) DiscountPercentage, sum(DiscountAmount) DiscountAmount &#xD;&#xA;                from TrxDiscounts td, trx_lines l &#xD;&#xA;                where l.TrxId = td.TrxId &#xD;&#xA;                    and l.lineId = td.LineId &#xD;&#xA;                group by DiscountId, td.TrxId, l.LineId) l,&#xD;&#xA;            trx_header v, discounts d, users us, postypes pt&#xD;&#xA;&#x9;&#x9;where trxdate &gt;= @fromdate and trxdate &lt;@todate &#xD;&#xA;        and v.trxid = l.trxid &#xD;&#xA;        and pt.postypeid = v.postypeid&#xD;&#xA;        and pt.postypename = 'IT3'&#xD;&#xA;        and l.discountid = d.discount_id &#xD;&#xA;        and us.user_id = v.user_id &#xD;&#xA;            group by discount_name, username &#xD;&#xA;            order by 1, 2&#xD;&#xA;" CommandTimeout="600" Name="dsDiscountBreakdown">
