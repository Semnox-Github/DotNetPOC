<?xml version="1.0" encoding="utf-8"?>
<Report Width="16in" Name="GameMetric" xmlns="http://schemas.telerik.com/reporting/2012/4.2">
  <DataSources>
    <SqlDataSource QueryDesignerState="null" ConnectionString="GameTimeHQ" SelectCommand="set @dfromDate = dbo.Converttoservertime(@fromDate, @offSet) &#xD;&#xA;set @dtoDate = dbo.Converttoservertime(@toDate, @offSet) &#xD;&#xA;&#xD;&#xA;select site_name,machine_id, game_profile_name as game_profile,&#xD;&#xA;    CASE WHEN machine_NAME LIKE '%-Hopper' THEN SUBSTRING(machine_name,1,CHARINDEX('-',machine_name)-1) ELSE machine_name END AS game_name, machine_address as ID,  &#xD;&#xA;    SUM(SIGN(credits)) AS credit_play_count,  &#xD;&#xA;    SUM(SIGN(courtesy)) AS crtsy_play_count,  &#xD;&#xA;    SUM(SIGN(bonus)) AS bonus_play_count,  &#xD;&#xA;    SUM(SIGN(time)) AS time_play_count,  &#xD;&#xA;    SUM(CASE WHEN MACHINE_NAME LIKE '%-Hopper' THEN 0 ELSE case when credits + courtesy + bonus + time = 0 then 1 else 0 end END) AS Card_game_count,  &#xD;&#xA;    COUNT(CASE WHEN MACHINE_NAME LIKE '%-Hopper' THEN NULL ELSE credits END) AS total_plays,  &#xD;&#xA;    SUM(credits) AS credits,  &#xD;&#xA;    SUM(courtesy) AS courtesy,  &#xD;&#xA;    SUM(bonus) AS bonus,  &#xD;&#xA;    SUM(time) AS time,  &#xD;&#xA;    SUM(cardGame) AS card_Game,  &#xD;&#xA;    SUM(credits + courtesy + bonus + time + cardGame) AS total_amount,  &#xD;&#xA;    SUM(ticket_count) AS tickets,  &#xD;&#xA;    SUM(ticket_cost) AS ticket_cost, SerialNumber SN,  &#xD;&#xA;    SUM(credits*@TaxPercent) AS GST&#xD;&#xA;from GameMetricView  g,site s&#xD;&#xA;where play_date &gt;= @dfromDate&#xD;&#xA;and play_date &lt; @dtoDate&#xD;&#xA;and technician_card = 'N'&#xD;&#xA;and (g.site_id=s.site_id or g.site_id is null)&#xD;&#xA;and ((g.site_id in (@site)) or (-1 in (@site)))&#xD;&#xA;and (game_profile_id in (@gameProfileSelection) or -1 in (@gameProfileSelection))&#xD;&#xA;group by site_name,game_profile_name, CASE WHEN machine_NAME LIKE '%-Hopper' THEN SUBSTRING(machine_name,1,CHARINDEX('-',machine_name)-1) ELSE machine_name END, machine_address, machine_id, SerialNumber&#xD;&#xA;union all&#xD;&#xA;select s.site_name,m.machine_id, profile_name,&#xD;&#xA;    m.machine_name, m.machine_address,&#xD;&#xA;    null,&#xD;&#xA;    null,&#xD;&#xA;    null,&#xD;&#xA;    null,&#xD;&#xA;    null,&#xD;&#xA;    null,&#xD;&#xA;    null,&#xD;&#xA;    null,&#xD;&#xA;    null,&#xD;&#xA;    null,&#xD;&#xA;    null,&#xD;&#xA;    null,&#xD;&#xA;    null,&#xD;&#xA;    null,&#xD;&#xA;m.SerialNumber,&#xD;&#xA;null&#xD;&#xA;from machines m left outer join GameMetricView v&#xD;&#xA;                            on play_date &gt;= @dfromDate&#xD;&#xA;                             and play_date &lt;= @dtoDate&#xD;&#xA;                             and technician_card = 'N'&#xD;&#xA;                             and v.machine_id = m.machine_id,&#xD;&#xA;                   games g, game_profile gpr   ,site s&#xD;&#xA;            where m.game_id = g.game_id&#xD;&#xA;            and g.game_profile_id = gpr.game_profile_id            &#xD;&#xA;            and (gpr.game_profile_id in (@gameProfileSelection) or -1 in (@gameProfileSelection))&#xD;&#xA;and ((m.site_id in (@site)) or (-1 in (@site)))&#xD;&#xA;and m.active_flag = 'Y'&#xD;&#xA;and v.machine_id is null&#xD;&#xA;and (m.site_id=s.site_id or m.site_id is null)" CommandTimeout="600" Name="dsGameMetricReport">
