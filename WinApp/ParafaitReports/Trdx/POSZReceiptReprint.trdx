(' ',(&#xD;&#xA;       CASE&#xD;&#xA;              WHEN Len(CONVERT(VARCHAR,Cast(Sum(posz_float_amount) AS MONEY),106)) &lt;= 6 THEN (17-( Len(CONVERT(VARCHAR,Cast(Sum(posz_float_amount) AS MONEY),106))))&#xD;&#xA;              ELSE (18                                                                          -( Len(CONVERT(VARCHAR,Cast(Sum(posz_float_amount) AS MONEY),106))))&#xD;&#xA;       END) -Len(CONVERT(VARCHAR,Cast(Sum(posz_float_amount) AS MONEY),106)))+CONVERT(VARCHAR,Cast(Sum(posz_float_amount) AS MONEY),106)&#xD;&#xA;FROM   (&#xD;&#xA;              SELECT Sum(&#xD;&#xA;                     CASE&#xD;&#xA;                            WHEN sf.shift_action='Paid Out' THEN -1*sf.shift_amount&#xD;&#xA;                            ELSE sf.shift_amount&#xD;&#xA;                     END ) posz_float_amount&#xD;&#xA;              FROM   shiftlog sf,&#xD;&#xA;                     shift s&#xD;&#xA;              WHERE  sf.shift_time BETWEEN @fromdate AND    @todate&#xD;&#xA;              AND    sf.shift_key=s.shift_key&#xD;&#xA;              AND    sf.shift_action IN('Paid In',&#xD;&#xA;                                        'Paid Out')&#xD;&#xA;              AND    s.pos_machine=@posmachine ) b&#xD;&#xA;UNION ALL&#xD;&#xA;SELECT 'Cash Sales',&#xD;&#xA;       '',&#xD;&#xA;       Replicate(' ',(&#xD;&#xA;       CASE&#xD;&#xA;              WHEN Len(CONVERT(VARCHAR,Cast(Sum(tp.amount) AS MONEY),106)) &lt;= 6 THEN (17-( Len(CONVERT(VARCHAR,Cast(Sum(tp.amount) AS MONEY),106))))&#xD;&#xA;              ELSE (18                                                                  -( Len(CONVERT(VARCHAR,Cast(Sum(tp.amount) AS MONEY),106))))&#xD;&#xA;       END) -Len(CONVERT(VARCHAR,Cast(Sum(tp.amount) AS MONEY),106)))+CONVERT(VARCHAR,Cast(Sum(tp.amount) AS MONEY),106) cash_amount&#xD;&#xA;FROM   trx_header h ,&#xD;&#xA;       trxpayments tp ,&#xD;&#xA;       paymentmodes p&#xD;&#xA;WHERE  h.trxid=tp.trxid&#xD;&#xA;AND    tp.paymentmodeid=p.paymentmodeid&#xD;&#xA;AND    paymentdate &gt;=@fromdate&#xD;&#xA;AND    paymentdate &lt;@todate&#xD;&#xA;AND    tp.posmachine IN (@posmachine)&#xD;&#xA;AND    p.paymentmode ='Cash'&#xD;&#xA;UNION ALL&#xD;&#xA;SELECT 'Total',&#xD;&#xA;       '',&#xD;&#xA;       Replicate(' ',(&#xD;&#xA;       CASE&#xD;&#xA;              WHEN Len(CONVERT(VARCHAR,Cast(Sum(diff) AS MONEY),106)) &lt;= 6 THEN (17-( Len(CONVERT(VARCHAR,Cast(Sum(diff) AS MONEY),106))))&#xD;&#xA;              ELSE (18                                                             -( Len(CONVERT(VARCHAR,Cast(Sum(diff) AS MONEY),106))))&#xD;&#xA;       END) -Len(CONVERT(VARCHAR,Cast(Sum(diff) AS MONEY),106)))+CONVERT(VARCHAR,Cast(Sum(diff) AS MONEY),106)&#xD;&#xA;FROM   (&#xD;&#xA;              SELECT&#xD;&#xA;                     (&#xD;&#xA;                            SELECT Sum(posz_float_amount)&#xD;&#xA;                            FROM   (&#xD;&#xA;                                          SELECT Sum(total) posz_float_amount&#xD;&#xA;                                          FROM   (&#xD;&#xA;                                                          SELECT   sf.shift_key,&#xD;&#xA;                                                                   Sum(&#xD;&#xA;                                                                   CASE&#xD;&#xA;                                                                            WHEN shift_action='close' THEN 0&#xD;&#xA;                                                                            WHEN sf.shift_action='Paid Out' THEN -1*sf.shift_amount&#xD;&#xA;                                                                            ELSE sf.shift_amount&#xD;&#xA;                                                                   END) total&#xD;&#xA;                                                          FROM     shiftlog sf&#xD;&#xA;                                                          WHERE    shift_time BETWEEN @fromdate AND      @todate&#xD;&#xA;                                                          GROUP BY sf.shift_key) sl ,&#xD;&#xA;                                                 shift s&#xD;&#xA;                                          WHERE  sl.shift_key=s.shift_key&#xD;&#xA;                                          AND    s.pos_machine=@posmachine ) B) +&#xD;&#xA;                     (&#xD;&#xA;                            SELECT Sum(tp.amount)&#xD;&#xA;                            FROM   trx_header h ,&#xD;&#xA;                                   trxpayments tp ,&#xD;&#xA;                                   paymentmodes p&#xD;&#xA;                            WHERE  h.trxid=tp.trxid&#xD;&#xA;                            AND    tp.paymentmodeid=p.paymentmodeid&#xD;&#xA;                            AND    paymentdate &gt;=@fromdate&#xD;&#xA;                            AND    paymentdate &lt;@todate&#xD;&#xA;                            AND    tp.posmachine IN (@posmachine)&#xD;&#xA;                            AND    p.paymentmode ='Cash' ) diff ) k&#xD;&#xA;UNION ALL&#xD;&#xA;SELECT Replicate(' ',12),&#xD;&#xA;       Replicate(' ',12),&#xD;&#xA;       Replicate(' ',12)&#xD;&#xA;UNION ALL&#xD;&#xA;--POSZ closing amount&#xD;&#xA;SELECT 'Shift Close',&#xD;&#xA;       '',&#xD;&#xA;       Replicate(' ',(&#xD;&#xA;       CASE&#xD;&#xA;              WHEN Len(CONVERT(VARCHAR,Cast(Sum(shift_amount) AS MONEY),106)) &lt;= 6 THEN (17-( Len(CONVERT(VARCHAR,Cast(Sum(shift_amount) AS MONEY),106))))&#xD;&#xA;              ELSE (18                                                                     -( Len(CONVERT(VARCHAR,Cast(Sum(shift_amount) AS MONEY),106))))&#xD;&#xA;       END) -Len(CONVERT(VARCHAR,Cast(Sum(shift_amount) AS MONEY),106)))+CONVERT(VARCHAR,Cast(Sum(shift_amount) AS MONEY),106)&#xD;&#xA;FROM   shift s&#xD;&#xA;WHERE  s.shift_time BETWEEN @fromdate AND    @todate&#xD;&#xA;AND    s.pos_machine=@posmachine&#xD;&#xA;AND    s.shift_action ='Close'&#xD;&#xA;AND    NOT EXISTS&#xD;&#xA;       (&#xD;&#xA;              SELECT 1&#xD;&#xA;              FROM   shift s1&#xD;&#xA;              WHERE  s1.shift_action='Close'&#xD;&#xA;              AND    s1.shift_time BETWEEN CONVERT(DATE,s.shift_time) AND    Dateadd(dd,1,CONVERT(DATE,s.shift_time))&#xD;&#xA;              AND    s1.shift_time &gt; s.shift_time&#xD;&#xA;              AND    s1.pos_machine=s.pos_machine )&#xD;&#xA;UNION ALL&#xD;&#xA;SELECT 'Difference',&#xD;&#xA;       '',&#xD;&#xA;       Replicate(' ',(&#xD;&#xA;       CASE&#xD;&#xA;              WHEN Len(CONVERT(VARCHAR,Cast(Sum(diff) AS MONEY),106)) &lt;= 6 THEN (17-( Len(CONVERT(VARCHAR,Cast(Sum(diff) AS MONEY),106))))&#xD;&#xA;              ELSE (18                                                             -( Len(CONVERT(VARCHAR,Cast(Sum(diff) AS MONEY),106))))&#xD;&#xA;       END) -Len(CONVERT(VARCHAR,Cast(Sum(diff) AS MONEY),106)))+CONVERT(VARCHAR,Cast(Sum(diff) AS MONEY),106)&#xD;&#xA;FROM   (&#xD;&#xA;              SELECT Sum(shift_amount-total) diff&#xD;&#xA;              FROM   (&#xD;&#xA;                              SELECT   sf.shift_key,&#xD;&#xA;                                       Sum(&#xD;&#xA;                                       CASE&#xD;&#xA;                                                WHEN sf.shift_action='close' THEN sf.actual_amount&#xD;&#xA;                                                WHEN sf.shift_action='Paid Out' THEN -1*sf.shift_amount&#xD;&#xA;                                                ELSE sf.shift_amount&#xD;&#xA;                                       END) total&#xD;&#xA;                              FROM     shiftlog sf&#xD;&#xA;                              WHERE    shift_time BETWEEN @fromdate AND      @todate&#xD;&#xA;                              GROUP BY sf.shift_key) sl ,&#xD;&#xA;                     shift s&#xD;&#xA;              WHERE  sl.shift_key=s.shift_key&#xD;&#xA;              AND    s.pos_machine=@posmachine ) b &#xD;&#xA;--begin&#xD;&#xA;--UPDATE posmachines&#xD;&#xA;--SET    dayendtime = @todate&#xD;&#xA;--WHERE  posname IN (@posmachine)&#xD;&#xA;--AND    ((&#xD;&#xA;--              dayendtime &lt; @todate&#xD;&#xA;--       AND    dayendtime &lt; daybegintime)&#xD;&#xA;--       OR     dayendtime IS NULL)&#xD;&#xA;--end" Name="dsPOSZ">
