entMode in('Cheque','Check') then 'Cheque'  else p.PaymentMode end PaymentMode ,sum(case when p.isCreditCard='Y' then (tp.amount+tp.TipAmount) else tp.Amount end) amount &#xD;&#xA; from trx_header t ,TrxPayments tp ,PaymentModes p,site s,users u&#xD;&#xA;where t.trxid=tp.trxid and tp.PaymentModeId=p.PaymentModeId &#xD;&#xA;and (t.site_id=s.site_id or t.site_id is null) &#xD;&#xA;and tp.CreatedBy=u.loginid and (u.site_id=tp.site_id or tp.site_id is null or u.site_id is null)&#xD;&#xA;and tp.PaymentDate &gt;=@dfromdate and tp.PaymentDate&lt; @dtodate&#xD;&#xA;and t.status not in ('cancelled')&#xD;&#xA;and (t.site_id in (@SiteId) or -1 in (@SiteId))&#xD;&#xA;group by site_name,(case when paymentmode='cash' then 1  else 4 end),case when  p.PaymentMode like 'Online%'  then 'Online Payment'&#xD;&#xA;when isCreditCard='y' then 'Credit/Debit' &#xD;&#xA;when isDebitCard='Y' then 'Game Card Amount'&#xD;&#xA;when p.PaymentMode in('Cheque','Check') then 'Cheque'  else p.PaymentMode end&#xD;&#xA;&#xD;&#xA;union all&#xD;&#xA; &#xD;&#xA;select 2 orderid,site_name ,'Tips',sum(tp.TipAmount)*-1 from trx_header t ,TrxPayments tp ,PaymentModes p,site s,users u&#xD;&#xA;where t.trxid=tp.trxid and tp.PaymentModeId=p.PaymentModeId &#xD;&#xA;and (t.site_id=s.site_id or t.site_id is null) &#xD;&#xA;and tp.CreatedBy=u.loginid and (u.site_id=tp.site_id or tp.site_id is null or u.site_id is null)&#xD;&#xA;and tp.PaymentDate &gt;=@dfromdate and tp.PaymentDate&lt;@dtodate&#xD;&#xA;and (t.site_id in (@SiteId) or -1 in (@SiteId))&#xD;&#xA;group by site_name&#xD;&#xA;&#xD;&#xA;union all&#xD;&#xA;&#xD;&#xA;select 3,a.site_name,'Total Cash',cash-Tip from (select site_name,s.site_id,sum(tp.amount)  cash &#xD;&#xA;from trx_header t ,TrxPayments tp ,PaymentModes p,site s,users u&#xD;&#xA;where t.trxid=tp.trxid and tp.PaymentModeId=p.PaymentModeId &#xD;&#xA;and (t.site_id=s.site_id or t.site_id is null) &#xD;&#xA;and tp.CreatedBy=u.loginid and (u.site_id=tp.site_id or tp.site_id is null or u.site_id is null)&#xD;&#xA;and tp.PaymentDate &gt;=@dfromdate and tp.PaymentDate&lt;@dtodate&#xD;&#xA;and paymentmode='cash'&#xD;&#xA;group by site_name,s.site_id) a,&#xD;&#xA;(select site_name,sum(tp.TipAmount) tip from trx_header t ,TrxPayments tp ,PaymentModes p,site s,users u&#xD;&#xA;where t.trxid=tp.trxid and tp.PaymentModeId=p.PaymentModeId and (t.site_id=s.site_id or t.site_id is null)&#xD;&#xA;and tp.CreatedBy=u.loginid and (u.site_id=tp.site_id or tp.site_id is null or u.site_id is null)&#xD;&#xA; and tp.PaymentDate &gt;=@dfromdate and tp.PaymentDate&lt;@dtodate&#xD;&#xA; group by site_name) b&#xD;&#xA;where a.site_name=b.site_name&#xD;&#xA;and (a.site_id in (@SiteId) or -1 in (@SiteId))&#xD;&#xA;&#xD;&#xA;union all&#xD;&#xA;&#xD;&#xA;select 5,a.site_name,'Total Collected',amount-Tip from (&#xD;&#xA;select site_name,s.site_id,sum(case when p.isCreditCard='Y' then (tp.amount+tp.TipAmount) else tp.Amount end)  Amount &#xD;&#xA;from trx_header t ,TrxPayments tp ,PaymentModes p,site s,users u&#xD;&#xA;where t.trxid=tp.trxid and tp.PaymentModeId=p.PaymentModeId &#xD;&#xA;and (t.site_id=s.site_id or t.site_id is null) &#xD;&#xA;and tp.CreatedBy=u.loginid and (u.site_id=tp.site_id or tp.site_id is null or u.site_id is null)&#xD;&#xA;and tp.PaymentDate &gt;=@dfromdate and tp.PaymentDate&lt;@dtodate&#xD;&#xA;&#xD;&#xA;and t.status not in ('cancelled') &#xD;&#xA;group by site_name,s.site_id) a,&#xD;&#xA;(select site_name,sum(tp.TipAmount) tip from trx_header t ,TrxPayments tp ,PaymentModes p,site s,users u&#xD;&#xA;where t.trxid=tp.trxid and tp.PaymentModeId=p.PaymentModeId and (t.site_id=s.site_id or t.site_id is null)&#xD;&#xA;and tp.CreatedBy=u.loginid and (u.site_id=tp.site_id or tp.site_id is null or u.site_id is null)&#xD;&#xA;and  tp.PaymentDate &gt;=@dfromdate and tp.PaymentDate&lt;@dtodate &#xD;&#xA;group by site_name) b&#xD;&#xA;where a.site_name=b.site_name&#xD;&#xA;and (a.site_id in (@SiteId) or -1 in (@SiteId))&#xD;&#xA;&#xD;&#xA;) pay on sa.site_name=pay.site_name  and   pay.PaymentMode=sa.payment&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;select * from #cte_paymmentTotal&#xD;&#xA;union all&#xD;&#xA;select orderid,site_name,TYPE,amount  from (&#xD;&#xA; select 6 orderid,site_name,TYPE,amount,maxcount-norows 'difference',row_number() over (partition by site_name order by site_name)rowno from &#xD;&#xA; (select site_name,count(distinct paymentmode) norows  from #cte_paymmentTotal &#xD;&#xA;group by site_name) A cross join  (select  11 maxcount) B&#xD;&#xA;cross join &#xD;&#xA;(select null TYPE,null amount&#xD;&#xA;union all&#xD;&#xA;select  null TYPE,null amount&#xD;&#xA;union all&#xD;&#xA;select  null TYPE,null amount&#xD;&#xA;union all&#xD;&#xA;select  null TYPE,null amount&#xD;&#xA;union all&#xD;&#xA;select  null TYPE,null amount&#xD;&#xA;union all&#xD;&#xA;select  null TYPE,null amount&#xD;&#xA;union all&#xD;&#xA;select null TYPE,null amount&#xD;&#xA;union all&#xD;&#xA;select null type,null amount&#xD;&#xA;union all&#xD;&#xA;select null type,null amount&#xD;&#xA;union all&#xD;&#xA;select null type,null amount&#xD;&#xA;union all&#xD;&#xA;select null type,null amount&#xD;&#xA;union all&#xD;&#xA;select  null type,null amount&#xD;&#xA;union all&#xD;&#xA;select  null type,null amount&#xD;&#xA;union all&#xD;&#xA;select  null type,null amount&#xD;&#xA;union all&#xD;&#xA;select  null type,null amount&#xD;&#xA;union all&#xD;&#xA;select null type,null amount&#xD;&#xA;union all&#xD;&#xA;select null type,null amount&#xD;&#xA;union all&#xD;&#xA;select null type,null amount&#xD;&#xA;union all&#xD;&#xA;select null type,null amount&#xD;&#xA;union all&#xD;&#xA;select null type,null amount) tp&#xD;&#xA;) tdp&#xD;&#xA;where rowno&lt;=[difference]&#xD;&#xA;order by 2,1" Name="dsPaymentTotal">
