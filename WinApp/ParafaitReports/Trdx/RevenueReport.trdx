    <SqlDataSource QueryDesignerState="null" ConnectionString="ServicechargeNGratuity" SelectCommand="&#xD;&#xA;set @dfromDate = dbo.Converttoservertime(@fromDate, @offSet) &#xD;&#xA;set @dtoDate = dbo.Converttoservertime(@toDate, @offSet) &#xD;&#xA;&#xD;&#xA;--declare @dfromdate datetime = '2023-3-12 6:00'&#xD;&#xA;--declare @dtodate datetime = '2023-03-13 6:00';&#xD;&#xA;--declare @offset int=0&#xD;&#xA;--declare @site int=-1&#xD;&#xA;SET FMTONLY OFF;&#xD;&#xA;&#xD;&#xA;IF OBJECT_ID('tempdb..#Tempdrawer ') IS NOT NULL DROP TABLE #Tempdrawer ;&#xD;&#xA;IF OBJECT_ID('tempdb..#Tempdrawernew ') IS NOT NULL DROP TABLE #Tempdrawernew ;&#xD;&#xA;IF OBJECT_ID('tempdb..#tempopenDRW ') IS NOT NULL DROP TABLE #tempopenDRW ;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;----For the Old record with  close records&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9; select shift_key,username,Shift_Type,&#xD;&#xA;&#x9;&#x9;&#x9;&#x9; lead(Shift_Type) over(Partition by Username,pos_machine order by Username,pos_machine,shift_time) Shift_Type1,shift_time,&#xD;&#xA;&#x9;&#x9;&#x9;&#x9; lead(shift_time) over(Partition by Username,pos_machine order by Username,pos_machine,shift_time) S_out,pos_machine ,Open_cash,&#xD;&#xA;&#x9;&#x9;&#x9;&#x9; lead(NetShift_cash) over(Partition by Username,pos_machine order by Username,pos_machine,shift_time) NetShift_cash,&#xD;&#xA;lead(system_Cash) over(Partition by Username,pos_machine  order by Username,shift_time) system_Cash,&#xD;&#xA;lead(System_Credit) over(Partition by Username,pos_machine order by Username,shift_time) System_Credit,&#xD;&#xA;lead(paid_out) over(Partition by Username,pos_machine order by Username,pos_machine,shift_time) paid_out,&#xD;&#xA;lead(ClosingAmount) over(Partition by Username,pos_machine order by Username,pos_machine,shift_time) ClosingAmount,&#xD;&#xA;lead(diff) over( Partition by Username,pos_machine order by Username,shift_time) diff into #Tempdrawer&#xD;&#xA;from &#xD;&#xA;(&#xD;&#xA;SELECT s.shift_key, username+' '+isnull(u.EmpLastName,'') [UserName],s.shift_action Shift_Type,s.shift_time,pos_machine,( CASE WHEN s.shift_action IN ( 'Open', 'ROpen' )&#xD;&#xA;                                 THEN  s.shift_amount ELSE 0 END ) [Open_cash],( CASE WHEN s.shift_action IN ( 'Open', 'ROpen' )&#xD;&#xA;                                 THEN  s.chequeamount + s.couponamount  ELSE 0 END )  [Open_Credit],s.actual_amount  [NetShift_cash],&#xD;&#xA;                               s.actualchequeamount + s.actualcouponamount  [NetShift_Credit],&#xD;&#xA;                               s.actual_amount  + Lag (( CASE WHEN s.shift_action IN ('Open', 'ROpen') THEN  s.shift_amount ELSE&#xD;&#xA;                               0 END), 1) OVER (ORDER BY pos_machine, shift_username,s.shift_time) AS [system_Cash],&#xD;&#xA;                               ( sf.shift_amount ) paid_out,&#xD;&#xA;                               CASE WHEN s.shift_action IN( 'close', 'RClose' ) THEN s.shift_amount ELSE 0 END ClosingAmount,&#xD;&#xA;                               s.actualchequeamount + s.actualcouponamount + Lag (( CASE WHEN s.shift_action IN ('Open','ROpen')&#xD;&#xA;                               THEN  s.chequeamount+s.couponamount ELSE 0 END), 1) OVER ( ORDER BY  pos_machine, shift_username, s.shift_time)&#xD;&#xA;                               AS [System_Credit],( CASE  WHEN s.shift_action IN( 'close', 'RClose' ) THEN  s.shift_amount ELSE 0 END ) - ( s.actual_amount&#xD;&#xA;                              + Lag (( CASE WHEN s.shift_action IN  ('Open' , 'ROpen') THEN s.shift_amount ELSE 0 END), 1) OVER (ORDER BY pos_machine, shift_username ,&#xD;&#xA;                                                     s.shift_time) ) diff&#xD;&#xA;                        FROM   shift s&#xD;&#xA;                               LEFT JOIN shiftlog sf&#xD;&#xA;                                      ON s.shift_key = sf.shift_key&#xD;&#xA;                                         AND sf.shift_action = 'paid out',users u&#xD;&#xA;                        WHERE  s.shift_time &gt;= @dfromdate AND s.shift_time &lt; @dtodate&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--and s.shift_username='Joanna'&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and s.ShiftLoginId=u.loginid  and (u.site_id=s.site_id or s.site_id is null)&#xD;&#xA;                                    &#xD;&#xA;                               AND ( s.site_id IN ( @Site )                                      OR -1 IN ( @Site ) )&#xD;&#xA;                               AND NOT EXISTS (SELECT 1&#xD;&#xA;                                               FROM   shift sh,&#xD;&#xA;                                                      shiftlog sl&#xD;&#xA;                                               WHERE  sh.shift_key =&#xD;&#xA;                                                      sl.shift_key&#xD;&#xA;                                                      AND sl.shift_time &gt;=&#xD;&#xA;        @dfromdate&#xD;&#xA;         &#xD;&#xA;        AND sl.shift_time &lt;&#xD;&#xA;        @dtodate&#xD;&#xA;         &#xD;&#xA;        AND sh.shift_action = 'close'&#xD;&#xA;        AND sl.shift_action like '%Open'&#xD;&#xA;        AND s.shift_key = sh.shift_key)&#xD;&#xA;        AND NOT EXISTS (SELECT 1&#xD;&#xA;        FROM   (SELECT s.shift_key,s.shift_action,&#xD;&#xA;            Lead (( CASE&#xD;&#xA;                      WHEN s.shift_action IN&#xD;&#xA;                           ( 'close', 'RClose'&#xD;&#xA;                           )&#xD;&#xA;                    THEN&#xD;&#xA;                      s.shift_amount&#xD;&#xA;                      ELSE 0&#xD;&#xA;                    END ), 1)&#xD;&#xA;              OVER (Partition by shift_username,pos_machine&#xD;&#xA;                ORDER BY pos_machine,&#xD;&#xA;              shift_username,&#xD;&#xA;              s.shift_time)&#xD;&#xA;            CloseValue&#xD;&#xA;        FROM   shift s&#xD;&#xA;        WHERE  s.shift_time &gt;=@dfromdate   AND s.shift_time &lt;  @dtodate&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;         )A&#xD;&#xA;        WHERE  shift_action like '%open'and  closevalue IS NULL &#xD;&#xA;        AND s.shift_key = A.shift_key)&#xD;&#xA;&#x9;&#x9;) A&#xD;&#xA;&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#xD;&#xA;&#x9;&#x9;----For the HE Version closed  records &#xD;&#xA;&#xD;&#xA;&#xD;&#xA;    SELECT shift_key,shift_username,shift_action,shift_action shift_action1,Intime, OutTime ,pos_machine, &#xD;&#xA;sum(isnull([transaction],0) + isnull(initial,0)) [Cash Counted], sum(isnull(paidout,0)) [Cash Pulled],&#xD;&#xA;sum(isnull(checkpulled,0)) [Checks Counted], sum(isnull(initial,0)) [Initial Drawer],&#xD;&#xA;sum(isnull([transaction],0)) [Total Deposit],sum(isnull(cashier,0)) ClosingAmount,&#xD;&#xA;sum(isnull([transaction],0) + isnull(initial,0) -isnull(cashier,0)) [Over Short] into #Tempdrawernew&#xD;&#xA;     FROM   (&#xD;&#xA;&#x9;&#x9;SELECT sl1.shift_key,u.username+' '+isnull(u.EmpLastName,'') shift_username, min(sl1.shift_time) over (partition by sl1.shift_key order by sl1.shift_key) Intime,&#xD;&#xA;&#x9;&#x9;sh1.shift_action , max(sl1.shift_time) over (partition by sl1.shift_key order by sl1.shift_key) OutTime,&#xD;&#xA;&#x9;&#x9;sh1.pos_machine,(CASE WHEN ( sh1.shift_action IN( 'close', 'Rclose' ) AND sl1.shift_action IN( 'close', 'Rclose' ) ) THEN&#xD;&#xA;        sh1.actual_amount ELSE 0 END) [Transaction],&#xD;&#xA;        (CASE WHEN sl1.shift_action = 'Paid out' THEN sl1.shift_amount END) paidOut,&#xD;&#xA;        (CASE WHEN ( sh1.shift_action IN( 'close', 'Rclose' ) AND sl1.shift_action IN( 'open', 'ROpen' ) ) THEN sh1.actualchequeamount&#xD;&#xA;        + sh1.actualcouponamount + sl1.chequeamount    + sl1.couponamount ELSE 0 END) [Checkpulled],&#xD;&#xA;        (CASE WHEN sl1.shift_action = 'Paid out' THEN -1 * sl1.shift_amount  WHEN sl1.shift_action IN( 'close', 'RClose' ) THEN 0 ELSE sl1.shift_amount END) Initial,&#xD;&#xA;        (CASE WHEN ( sh1.shift_action IN( 'close', 'RClose' ) AND sl1.shift_action IN( 'close', 'Rclose' ) ) THEN sh1.shift_amount ELSE 0 END) cashier&#