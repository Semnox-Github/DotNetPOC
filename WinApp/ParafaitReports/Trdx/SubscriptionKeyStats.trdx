    <SqlDataSource QueryDesignerState="null" ConnectionString="Parafaitsubscription" SelectCommand="select s.site_name, count(distinct customerId) ActiveCustomers, count(subscriptionheaderId) ActiveSubscriptions&#xD;&#xA; from SubscriptionHeader sh, site s&#xD;&#xA;where sh.Status = 'ACTIVE'  &#xD;&#xA;and (s.site_id = sh.site_id or sh.site_id is null)&#xD;&#xA;and  ( sh.site_id IN ( @Site ) OR -1 IN ( @Site ) )&#xD;&#xA;group by  s.site_name" Name="dsActiveCustomers">
