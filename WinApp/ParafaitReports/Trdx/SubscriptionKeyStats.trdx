    </SqlDataSource>
    <SqlDataSource QueryDesignerState="null" ConnectionString="Parafaitsubscription" SelectCommand="SET @startOfMonth = (SELECT DATEADD(DAY,1,EOMONTH(getdate(),-1)))&#xD;&#xA;set @tempDate = (SELECT DATEADD(DAY,-1,EOMONTH(@startOfMonth,-1)))&#xD;&#xA;set @startOfPrevMonth = (SELECT DATEADD(DAY,1,EOMONTH(@tempDate,-1)))&#xD;&#xA;set @countBeginOfLastMonth = (select count(1) from subscriptionHeader sh where sh.CreationDate &lt; @startOfPrevMonth &#xD;&#xA;                              and ( sh.site_id IN ( @Site ) OR -1 IN ( @Site )))&#xD;&#xA;set @countEndOfLastMonth = (select count(1)  from subscriptionHeader sh where sh.CreationDate &lt; @startOfMonth  &#xD;&#xA;                            and ( sh.site_id IN ( @Site ) OR -1 IN ( @Site )))&#xD;&#xA;SET @divserValue = (SELECT CASE WHEN @countBeginOfLastMonth &gt; 0 THEN @countBeginOfLastMonth ELSE 1 END)&#xD;&#xA;SET @subscriptionChurn = (Select (@countBeginOfLastMonth - @countEndOfLastMonth)/@divserValue)&#xD;&#xA;&#xD;&#xA;Select CASE WHEN @subscriptionChurn &gt; -1 THEN @subscriptionChurn ELSE 0 END as 'Subscription Churn',&#xD;&#xA;       @countBeginOfLastMonth as 'Subscription Count - Beginning Of Previous Month',&#xD;&#xA;&#x9;   @countEndOfLastMonth as 'Subscription Count - End Of Previous Month'" Name="dsSubscriptionMonthlyChurnRate">
      <Parameters>
        <SqlDataSourceParameter DbType="DateTime" Name="@startOfMonth" />
        <SqlDataSourceParameter DbType="DateTime" Name="@tempDate" />
        <SqlDataSourceParameter DbType="DateTime" Name="@startOfPrevMonth" />
        <SqlDataSourceParameter DbType="Int32" Name="@countBeginOfLastMonth" />
        <SqlDataSourceParameter DbType="Int32" Name="@Site">
          <Value>
            <String>= Parameters.site.Value</String>
          </Value>
        </SqlDataSourceParameter>
        <SqlDataSourceParameter DbType="Int32" Name="@countEndOfLastMonth" />
        <SqlDataSourceParameter DbType="Int32" Name="@divserValue" />
        <SqlDataSourceParameter DbType="Double" Name="@subscriptionChurn" />
      </Parameters>
    </SqlDataSource>
    <SqlDataSource QueryDesignerState="null" ConnectionString="Parafaitsubscription" SelectCommand="SET @startOfMonth = (SELECT DATEADD(DAY,1,EOMONTH(getdate(),-1)))&#xD;&#xA;set @tempDate = (SELECT DATEADD(DAY,-1,EOMONTH(@startOfMonth,-1)))&#xD;&#xA;set @startOfPrevMonth = (SELECT DATEADD(DAY,1,EOMONTH(@tempDate,-1)))&#xD;&#xA;set @custCountBeginOfLastMonth = (select count(distinct customerId) from subscriptionHeader sh where sh.CreationDate &lt; @startOfPrevMonth &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and ( sh.site_id IN ( @Site ) OR -1 IN ( @Site )))&#xD;&#xA;set @custCountEndOfLastMonth = (select count(distinct customerId) from subscriptionHeader sh where sh.CreationDate &lt; @startOfMonth &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and ( sh.site_id IN ( @Site ) OR -1 IN ( @Site )))&#xD;&#xA;SET @divserValue = (SELECT CASE WHEN @custCountBeginOfLastMonth &gt; 0 THEN @custCountBeginOfLastMonth ELSE 1 END)&#xD;&#xA;SET @custChurn = (Select (@custCountBeginOfLastMonth - @custCountEndOfLastMonth)/@divserValue)&#xD;&#xA;&#xD;&#xA;Select CASE WHEN @custChurn &gt; -1 THEN @custChurn ELSE 0 END as 'Customer Churn'&#xD;&#xA;       , @custCountBeginOfLastMonth as 'Customer Count - Begining Of Previous Month'&#xD;&#xA;&#x9;   , @custCountEndOfLastMonth as 'Customer Count - End Of Previous Month'" Name="dsCustomersMonthlyChurnRate">
      <Parameters>
        <SqlDataSourceParameter DbType="DateTime" Name="@startOfMonth" />
        <SqlDataSourceParameter DbType="DateTime" Name="@tempDate" />
        <SqlDataSourceParameter DbType="DateTime" Name="@startOfPrevMonth" />
        <SqlDataSourceParameter DbType="Int32" Name="@custCountBeginOfLastMonth" />
        <SqlDataSourceParameter DbType="Int32" Name="@Site" />
        <SqlDataSourceParameter DbType="Int32" Name="@custCountEndOfLastMonth" />
        <SqlDataSourceParameter DbType="Int32" Name="@divserValue" />
        <SqlDataSourceParameter DbType="Double" Name="@custChurn" />
      </Parameters>
    </SqlDataSource>
    <SqlDataSource QueryDesignerState="null" ConnectionString="Parafaitsubscription" SelectCommand="SET @startOfThisMonth = (SELECT DATEADD(DAY,1,EOMONTH(getdate(),-1)))&#xD;&#xA;SET @startOfNextMonth = (SELECT DATEADD(DAY,1,EOMONTH(getdate(),0)))&#xD;&#xA;&#xD;&#xA;select s.site_name, SUM(ISNULL(sbs.OverridedBillAmount,sbs.BillAmount)) as MonthlyRevenue&#xD;&#xA; from SubscriptionBillingSchedule sbs, site s&#xD;&#xA;where&#xD;&#xA; (s.site_id = sbs.site_id or sbs.site_id is null)&#xD;&#xA; and sbs.Status = 'ACTIVE'&#xD;&#xA; and BillOnDate &gt;= @startOfThisMonth&#xD;&#xA; and BillOnDate &lt; @startOfNextMonth&#xD;&#xA; and  ( sbs.site_id IN ( @Site ) OR -1 IN ( @Site ) )&#xD;&#xA; group by s.site_name" Name="dsMonthlyRecurringRevenue">
