;&#xA;from shift s left join shiftlog sf on s.shift_key=sf.shift_key and sf.shift_action='Paid Out'&#xD;&#xA;where s.shift_time&gt;=dbo.ConvertToServerTime(@fromDate,@offSet) and s.shift_time &lt;dbo.ConvertToServerTime(@toDate,@offSet)&#xD;&#xA;and (s.site_id in (@Site) or -1 in (@Site))&#xD;&#xA;union all&#xD;&#xA;&#xD;&#xA;select 3,'Paid_In',isnull(sum(sf1.shift_amount),0) paid_In&#xD;&#xA;from shift s &#xD;&#xA;left join shiftlog sf1 on s.shift_key=sf1.shift_key and sf1.shift_action='Paid In'&#xD;&#xA;where s.shift_time&gt;=dbo.ConvertToServerTime(@fromDate,@offSet) and s.shift_time &lt;dbo.ConvertToServerTime(@toDate,@offSet)&#xD;&#xA;and (s.site_id in (@Site) or -1 in (@Site))&#xD;&#xA;union all&#xD;&#xA;&#xD;&#xA;select 2,'Credit Card Tips' as 'Category', sum(tipamount) Amount from (&#xD;&#xA;select  Convert(varchar, dateadd(hour, -6, trxdate), 102) TransactionDate,&#xD;&#xA;u.username+' '+u.EmpLastName username,u.EmpNumber,&#xD;&#xA;       (case when pm.isCreditCard='Y' then (case when ccpgw.UserTraceData='' or ccpgw.UserTraceData is null or ccpgw.UserTraceData ='Unknown' &#xD;&#xA;then isnull(tp.CreditCardName,pm.paymentmode) else ccpgw.usertracedata end )else pm.PaymentMode end) paymentmode,tp.PosMachine,&#xD;&#xA;sum(tp.amount) amount,sum(tp.tipamount) tipamount&#xD;&#xA;  from trxpayments tp  left outer join CCTransactionsPGW ccpgw on ccpgw.ResponseID=tp.CCResponseId,&#xD;&#xA;       PaymentModes pm,&#xD;&#xA;&#x9;   trx_header th,&#xD;&#xA;&#x9;   users u&#xD;&#xA; where tp.PaymentModeId = pm.PaymentModeId&#xD;&#xA;   and tp.TrxId = th.trxid&#xD;&#xA;   and tp.paymentdate between dbo.ConvertToServerTime(@fromDate,@offSet)  and dbo.ConvertToServerTime(@toDate,@offSet)&#xD;&#xA;   and u.user_id = th.user_id&#xD;&#xA;   and (tp.site_id in (@Site) or -1 in (@Site))&#xD;&#xA; group by Convert(varchar, dateadd(hour, -6, trxdate), 102),tp.PosMachine,u.EmpNumber,u.username+' '+u.EmpLastName, &#xD;&#xA;         (case when pm.isCreditCard='Y' then (case when ccpgw.UserTraceData='' or ccpgw.UserTraceData is null or ccpgw.UserTraceData ='Unknown' &#xD;&#xA;then isnull(tp.CreditCardName,pm.paymentmode) else ccpgw.usertracedata end )else pm.PaymentMode end)&#xD;&#xA;having sum(tp.amount) &lt;&gt; 0) a where PosMachine not in ('Kiosk1', 'Kiosk2') and PosMachine in ('POS6','POS7','POS8','POS9')&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;)&#xD;&#xA;&#xD;&#xA;select *  from (select * from  Shift_user&#xD;&#xA;union all&#xD;&#xA;select 5 sortid,'Cash Deposit to Safe', sum(case when type in('Cash Collection','Paid_In') then Amount else -1*Amount end )&#xD;&#xA;from shift_user) A&#xD;&#xA;order by Sortid&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;" Name="CashTosafe">
