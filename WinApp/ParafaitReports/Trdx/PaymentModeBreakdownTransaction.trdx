unt = 0 then tp.amount else total_amount end )) end), 0) &#xD;&#xA;                             + Isnull(trxrv.deposit_amount*(case when tp.amount =0 then 1 else (tp.amount/(case when total_amount = 0 then tp.amount else total_amount end )) end), 0) &#xD;&#xA;                             ELSE 0 &#xD;&#xA;                           END)      Deposit, &#xD;&#xA;                       Sum(CASE isdebitcard &#xD;&#xA;                             WHEN 'N' THEN tp.amount - &#xD;&#xA;                                           Isnull(trxv.deposit_amount*( case when tp.amount =0 then 1 else (tp.amount/(case when total_amount = 0 then tp.amount else total_amount end )) end), 0) - &#xD;&#xA;                                           Isnull(trxrv.deposit_amount*(case when tp.amount =0 then 1 else (tp.amount/(case when total_amount = 0 then tp.amount else total_amount end )) end), 0) - &#xD;&#xA;                                                           taxamount * ( 1 - &#xD;&#xA;        Isnull(v.trxdiscountpercentage, 0) / 100.0 ) &#xD;&#xA;        * &#xD;&#xA;        tp.amount / CASE &#xD;&#xA;        trxnetamount &#xD;&#xA;        WHEN 0 THEN NULL &#xD;&#xA;        ELSE trxnetamount &#xD;&#xA;        END &#xD;&#xA;        ELSE 0 &#xD;&#xA;        END)      net_amount, &#xD;&#xA;        Sum(taxamount * ( 1 - Isnull(v.trxdiscountpercentage, 0) / 100.0 ) * &#xD;&#xA;            tp.amount &#xD;&#xA;            / &#xD;&#xA;        CASE trxnetamount &#xD;&#xA;        WHEN 0 THEN NULL &#xD;&#xA;        ELSE trxnetamount &#xD;&#xA;        END)      tax, &#xD;&#xA;        Sum(CASE &#xD;&#xA;        WHEN ( isroundoff = 'Y' &#xD;&#xA;        AND tp.amount BETWEEN -1 AND 1 ) THEN tp.amount &#xD;&#xA;        ELSE 0 &#xD;&#xA;        END) * -1 Round_Off, &#xD;&#xA;        Sum(CASE isdebitcard &#xD;&#xA;        WHEN 'N' THEN tp.amount &#xD;&#xA;        ELSE 0 &#xD;&#xA;        END)      gross_total, &#xD;&#xA;        CASE &#xD;&#xA;        WHEN iscash = 'Y' THEN 'Y' &#xD;&#xA;        ELSE &#xD;&#xA;        CASE &#xD;&#xA;        WHEN isroundoff = 'Y' THEN 'Y' &#xD;&#xA;        ELSE 'N' &#xD;&#xA;        END &#xD;&#xA;        END           isCash, &#xD;&#xA;        isdebitcard, &#xD;&#xA;        Sum(CASE &#xD;&#xA;        WHEN CONVERT(DATE, tp.paymentdate) &lt; &#xD;&#xA;        @dfromDate&#xD;&#xA;        AND CONVERT(DATE, tp.paymentdate) &lt; CONVERT(DATE, v.trxdate) THEN &#xD;&#xA;        tp.amount &#xD;&#xA;        ELSE 0 &#xD;&#xA;        END)      Redeem, &#xD;&#xA;        0             Advance &#xD;&#xA;        FROM   trx_header v &#xD;&#xA;               LEFT OUTER JOIN (SELECT trxid, &#xD;&#xA;                                       Sum(( net_amount * ( &#xD;&#xA;                                             cashratio + creditcardratio &#xD;&#xA;                                                          ) )) &#xD;&#xA;                                                           deposit_amount &#xD;&#xA;                                FROM   transactionview v &#xD;&#xA;                                WHERE  trxdate &gt;= &#xD;&#xA;@dfromDate&#xD;&#xA;AND trxdate &lt; &#xD;&#xA;    @dtoDate&#xD;&#xA;AND ( ( v.site_id IN ( @site ) ) &#xD;&#xA;       OR ( -1 IN ( @site ) ) ) &#xD;&#xA;AND ( v.user_id IN ( @userId ) &#xD;&#xA;       OR -1 IN ( @userId ) ) &#xD;&#xA;AND ( pos_machine IN ( @pos ) ) &#xD;&#xA;AND product_type IS NOT NULL &#xD;&#xA;AND product_type != 'LOYALTY' &#xD;&#xA;AND product_type = 'CARDDEPOSIT' &#xD;&#xA;GROUP  BY trxid) trxv &#xD;&#xA;ON trxv.trxid = v.trxid &#xD;&#xA;LEFT OUTER JOIN (SELECT &#xD;&#xA;             trxid, &#xD;&#xA;Sum(( net_amount * ( cashratio + creditcardratio &#xD;&#xA;                   ) )) &#xD;&#xA;             deposit_amount &#xD;&#xA;FROM   transactionview v &#xD;&#xA;WHERE  trxdate &gt;= &#xD;&#xA;@dfromDate&#xD;&#xA;AND trxdate &lt; &#xD;&#xA;   @dtoDate&#xD;&#xA;AND ( ( v.site_id IN ( @site ) ) &#xD;&#xA;       OR ( -1 IN ( @site ) ) ) &#xD;&#xA;AND ( v.user_id IN ( @userId ) &#xD;&#xA;       OR -1 IN 