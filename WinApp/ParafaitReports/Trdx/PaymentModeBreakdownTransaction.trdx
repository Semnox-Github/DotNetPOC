ode &#xD;&#xA;WHERE  th.trxid = tp.trxid &#xD;&#xA;AND tp.paymentmodeid = p.paymentmodeid &#xD;&#xA;AND tp.paymentdate &gt;= @dfromDate&#xD;&#xA;AND tp.paymentdate &lt; @dtoDate &#xD;&#xA;AND NOT ( trxdate &gt;= @dfromDate&#xD;&#xA;AND trxdate &lt; @dtoDate)&#xD;&#xA;AND ( ( th.site_id IN ( @site ) ) &#xD;&#xA;       OR ( -1 IN ( @site ) ) )  &#xD;&#xA;AND ( th.user_id IN ( @userId ) &#xD;&#xA;OR -1 IN ( @userId ) ) &#xD;&#xA;AND ( pos_machine IN (@pos))&#xD;&#xA;GROUP  BY paymentmode, &#xD;&#xA;iscash, &#xD;&#xA;iscreditcard, &#xD;&#xA;isdebitcard, &#xD;&#xA;isroundoff, &#xD;&#xA;cashmode) v &#xD;&#xA;GROUP  BY mode, &#xD;&#xA;          iscash, &#xD;&#xA;          isdebitcard &#xD;&#xA;  union all&#xD;&#xA;&#xD;&#xA;SELECT 'Gratuity' ,SUM(GratuityAmount*(GameCardRatio)),0,SUM(GratuityAmount*(cashratio+creditcardratio))*-1 ,0,SUM(GratuityAmount*(cashratio+creditcardratio))*-1,0 ,0,0,&#xD;&#xA;SUM(GratuityAmount*(cashratio+creditcardratio))*-1  FROM transactionview v,site s&#xD;&#xA;  WHERE  v.trxdate between @dfromdate&#xD;&#xA; and @dtodate&#xD;&#xA; and (v.site_id = s.site_id or v.site_id is null)&#xD;&#xA; and (v.site_id in (@Site) or -1 in (@Site))&#xD;&#xA; and v.status NOT IN ('OPEN','RESERVED','BOOKING','pending','CANCELLED','Initiated','Ordered','Delivered')&#xD;&#xA;AND product_type  in('GRATUITY')&#xD;&#xA;AND ( v.user_id IN ( @userId ) OR -1 IN ( @userId ) ) &#xD;&#xA;AND ( pos_machine IN ( @pos ) ) &#xD;&#xA;&#xD;&#xA;) A&#xD;&#xA;&#xD;&#xA;select * from  #tempPayment&#xD;&#xA;union all&#xD;&#xA;select 'Total',sum(void_amount),sum(Deposit),sum(tax_sales),sum(tax),sum(Gross_Sales),sum(round_off),sum(Redeem),sum(Advance),sum(Collection) from #tempPayment&#xD;&#xA;&#xD;&#xA; union all&#xD;&#xA;&#xD;&#xA;SELECT 'ServiceCharge' ,SUM((ServiceChargesAmount+isnull(taxonservicecharges,0))*(GameCardRatio))*-1,0,SUM((ServiceChargesAmount)*(cashratio+creditcardratio))*-1 ,sum(isnull(taxonservicecharges,0)*(cashratio+creditcardratio))*-1,&#xD;&#xA;SUM((ServiceCharges