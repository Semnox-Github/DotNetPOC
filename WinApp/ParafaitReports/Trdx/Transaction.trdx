    <SqlDataSource QueryDesignerState="null" ConnectionString="Parafait" SelectCommand="--declare @fromdate datetime = '2022-06-27 6:00'&#xD;&#xA;--declare @todate datetime = '2022-6-28 6:00';&#xD;&#xA;--DECLARE @site int=-1&#xD;&#xA;--declare @offset int=0&#xD;&#xA;--declare @userid int=-1&#xD;&#xA;--declare @dfromdate datetime &#xD;&#xA;--declare @dtodate datetime&#xD;&#xA;&#xD;&#xA;EXEC dbo.Setcontextinfo  @loginUserId; &#xD;&#xA;IF OBJECT_ID('tempdb..#tempCashier') IS NOT NULL DROP Table #tempCashier&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;set @dfromDate = dbo.Converttoservertime(@fromDate, @offSet) &#xD;&#xA;set @dtoDate = dbo.Converttoservertime(@toDate, @offSet) &#xD;&#xA;&#xD;&#xA;SELECT  1 orderid,username                                      AS UserName, &#xD;&#xA;       loginid, &#xD;&#xA;       Count(DISTINCT v.trxid)                       ops, &#xD;&#xA;       Sum(cnt) &#xD;&#xA;       + (SELECT Isnull(Sum(1), 0) transfers &#xD;&#xA;          FROM   tasks t, &#xD;&#xA;                 cards c &#xD;&#xA;          WHERE  transfer_to_card_id = c.card_id &#xD;&#xA;                 AND task_date &gt;= @dfromDate&#xD;&#xA;                 AND task_date &lt; @dtoDate &#xD;&#xA;                 AND c.issue_date &lt; @dfromDate &#xD;&#xA;                 AND ( ( t.site_id IN ( @site ) ) &#xD;&#xA;                        OR ( -1 IN ( @site ) ) ) &#xD;&#xA;                 AND t.user_id = v.user_id) - (SELECT Count(DISTINCT card_id) &#xD;&#xA;                                               -- reversals &#xD;&#xA;                                               FROM   transactionview tvr &#xD;&#xA;                                               WHERE &#xD;&#xA;       trxdate &gt;= @dfromDate&#xD;&#xA;       AND trxdate &lt; @dtoDate&#xD;&#xA;       AND ( ( tvr.site_id IN ( @site ) ) &#xD;&#xA;              OR ( -1 IN ( @site ) ) ) &#xD;&#xA;       AND quantity &lt; 0 &#xD;&#xA;       AND tvr.user_id = v.user_id --05-Jun-2015 condition added &#xD;&#xA;       AND NOT EXISTS (SELECT 1 &#xD;&#xA;                       FROM   products pr, &#xD;&#xA;                              product_type pt &#xD;&#xA;                       WHERE  pr.product_type_id = pt.product_type_id &#xD;&#xA;                              AND tvr.product_id = pr.product_id &#xD;&#xA;                              AND pt.product_type = 'GENERICSALE') &#xD;&#xA;       --Added on 27-Mar-2020 to exclude Load Bonus Task &#xD;&#xA;       AND product_type IN ( 'NEW', 'CARDDEPOSIT' )) new_cards, &#xD;&#xA;       Sum(void_amount)                    void_amount, &#xD;&#xA;       Sum(gross_total)                    gross_total, &#xD;&#xA;       v.user_id into #tempCashier&#xD;&#xA;FROM   (SELECT v.trxid, &#xD;&#xA;               v.user_id, &#xD;&#xA;               CONVERT(DECIMAL(18, 8), Sum((amount +ServiceChargesAmount+isnull(TaxOnServiceCharges,0)+GratuityAmount)* gamecardratio)) &#xD;&#xA;                      void_amount, &#xD;&#xA;               CONVERT(DECIMAL(18, 8), Sum((amount +ServiceChargesAmount+isnull(TaxOnServiceCharges,0)+GratuityAmount) * ( cashratio + &#xD;&#xA;                                                            creditcardratio &#xD;&#xA;                                                          ))) &#xD;&#xA;                      gross_total, &#xD;&#xA;               Count(DISTINCT new_card_id) - Count(DISTINCT refund_card_id) &#xD;&#xA;               cnt, &#xD;&#xA;               username, &#xD;&#xA;               loginid &#xD;&#xA;        FROM   transactionview v, &#xD;&#xA;               users u &#xD;&#xA;        WHERE  trxdate &gt;= @dfromDate&#xD;&#xA;               AND trxdate &lt; @dtoDate&#xD;&#xA;               AND u.user_id = v.user_id &#xD;&#xA;               AND ( ( v.site_id IN ( @site ) ) &#xD;&#xA;                      OR ( -1 IN ( @site ) ) ) &#xD;&#xA;&#x9;       AND ( v.user_id IN ( @userId ) &#x9;&#x9;      OR -1 IN ( @userId ) ) &#xD;&#xA;&#x9;&#x9;       AND ( pos_machine IN ( @pos ) ) &#xD;&#xA;               AND status = 'closed' &#xD;&#xA;               AND NOT EXISTS (SELECT 1 &#xD;&#xA;                               FROM   products pr, &#xD;&#xA;                                      product_type pt &#xD;&#xA;                               WHERE  pr.product_type_id = pt.product_type_id &#xD;&#xA;                                      AND v.product_id = pr.product_id &#xD;&#xA;                                      AND pt.product_type = 'GENERICSALE') &#xD;&#xA;        GROUP  BY v.trxid, &#xD;&#xA;                  username, &#xD;&#xA;                  v.user_id, &#xD;&#xA;                  loginid) v &#xD;&#xA;GROUP  BY username, &#xD;&#xA;          user_id, &#xD;&#xA;          loginid &#xD;&#xA;&#x9;&#x9; &#xD;&#xA;&#x9;&#x9;  &#x9;  union all&#xD;&#xA;&#xD;&#xA;SELECT 2 orderid,'Gratuity' ,Null,Null,0,0,SUM(GratuityAmount)*-1 ,Null FROM transactionview v,site s&#xD;&#xA;  WHERE  v.trxdate between @dfromdate&#xD;&#xA; and @dtodate&#xD;&#xA; and (v.site_id = s.site_id or v.site_id is null)&#xD;&#xA; and (v.site_id in (@Site) or -1 in (@Site))&#xD;&#xA; and v.status NOT IN ('OPEN','RESERVED','BOOKING','pending','CANCELLED','Initiated','Ordered','Delivered')&#xD;&#xA;AND product_type  in('GRATUITY')&#xD;&#xA;     AND ( v.user_id IN ( @userId ) &#x9;&#x9;      OR -1 IN ( @userId ) ) &#xD;&#xA;&#x9;&#x9;       AND ( pos_machine IN ( @pos ) ) &#xD;&#xA;&#xD;&#xA;select * from #tempCashier&#xD;&#xA;union all&#xD;&#xA;select 3,'Total',Null,sum(isnull(ops,0)),sum(new_cards),sum(void_amount),sum(gross_total),Null from #tempCashier&#xD;&#xA;union all&#xD;&#xA;&#xD;&#xA;SELECT 4 order1,'Service Charge' ,Null,Null,0,0,SUM(ServiceChargesAmount+isnull(v.TaxOnServiceCharges,0))*-1,Null FROM transactionview v,site s&#xD;&#xA;  WHERE  v.trxdate between @dfromdate&#xD;&#xA; and @dtodate&#xD;&#xA; and (v.site_id = s.site_id or v.site_id is null)&#xD;&#xA; and (v.site_id in (@Site) or -1 in (@Site))&#xD;&#xA; and v.status NOT IN ('OPEN','RESERVED','BOOKING','pending','CANCELLED','Initiated','Ordered','Delivered')&#xD;&#xA;AND product_type  in('SERVICECHARGE')&#xD;&#xA;     AND ( v.user_id IN ( @userId ) &#x9;&#x9;      OR -1 IN ( @userId ) ) &#xD;&#xA;&#x9;&#x9;     AND ( pos_machine IN ( @pos ) ) &#xD;&#xA;&#xD;&#xA;&#xD;&#xA;ORDER  BY 1,2 ,3" CommandTimeout="600" Name="dsCashierBreakdown">
