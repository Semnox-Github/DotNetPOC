    <SqlDataSource QueryDesignerState="null" ConnectionString="ServicechargeNGratuity" SelectCommand="set @dfromDate = dbo.Converttoservertime(@fromDate, @offSet) &#xD;&#xA;set @dtoDate = dbo.Converttoservertime(@toDate, @offSet) &#xD;&#xA;&#xD;&#xA;SELECT discount_name, &#xD;&#xA;       us.username, &#xD;&#xA;       Count(DISTINCT v.trxid)                                  ops, &#xD;&#xA;       Sum(l.discountamount / ( CASE discountpercentage &#xD;&#xA;                                  WHEN 0 THEN NULL &#xD;&#xA;                                  ELSE discountpercentage &#xD;&#xA;                                END ) * 100)                    original_amount, &#xD;&#xA;       Sum(l.discountamount)                                    discount, &#xD;&#xA;       Sum(l.discountamount / ( CASE discountpercentage &#xD;&#xA;                                  WHEN 0 THEN NULL &#xD;&#xA;                                  ELSE discountpercentage &#xD;&#xA;                                END ) * 100 - l.discountamount) charged &#xD;&#xA;FROM   (SELECT discountid, &#xD;&#xA;               td.trxid, &#xD;&#xA;               l.lineid, &#xD;&#xA;               Sum(discountpercentage) DiscountPercentage, &#xD;&#xA;               Sum(discountamount)     DiscountAmount &#xD;&#xA;        FROM   trxdiscounts td, &#xD;&#xA;               trx_lines l &#xD;&#xA;        WHERE  l.trxid = td.trxid &#xD;&#xA;               AND l.lineid = td.lineid &#xD;&#xA;               AND ( ( l.site_id IN ( @site ) ) &#xD;&#xA;                      OR ( -1 IN ( @site ) ) ) &#xD;&#xA;        GROUP  BY discountid, &#xD;&#xA;                  td.trxid, &#xD;&#xA;                  l.lineid) l, &#xD;&#xA;       trx_header v, &#xD;&#xA;       discounts d, &#xD;&#xA;       users us &#xD;&#xA;WHERE  trxdate &gt;= @dfromDate&#xD;&#xA;       AND trxdate &lt; @dtoDate&#xD;&#xA;       AND v.trxid = l.trxid &#xD;&#xA;       AND l.discountid = d.discount_id &#xD;&#xA;       AND us.user_id = v.user_id &#xD;&#xA;       AND v.status = 'CLOSED' &#xD;&#xA;       AND ( ( v.site_id IN ( @site ) ) &#xD;&#xA;              OR ( -1 IN ( @site ) ) ) &#xD;&#xA;       AND ( v.user_id IN ( @userId ) &#xD;&#xA;              OR -1 IN ( @userId ) ) &#xD;&#xA;       AND ( pos_machine IN ( @pos ) )       &#xD;&#xA;GROUP  BY discount_name, &#xD;&#xA;          username &#xD;&#xA;ORDER  BY 1, &#xD;&#xA;          2 " CommandTimeout="600" Name="dsDiscountBreakdown">
