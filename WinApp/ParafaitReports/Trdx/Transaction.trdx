    <SqlDataSource QueryDesignerState="null" ConnectionString="Parafait" SelectCommand="--declare @fromdate datetime = '2022-06-27 6:00'&#xD;&#xA;--declare @todate datetime = '2022-6-28 6:00';&#xD;&#xA;--DECLARE @site int=-1&#xD;&#xA;--declare @offset int=0&#xD;&#xA;--declare @userid int=-1&#xD;&#xA;--declare @pos varchar(20)='Guru A POS'&#xD;&#xA;--declare @dfromdate datetime &#xD;&#xA;--declare @dtodate datetime&#xD;&#xA;IF OBJECT_ID('tempdb..#tempPayment') IS NOT NULL DROP Table #tempPayment&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;set @dfromDate = dbo.Converttoservertime(@fromDate, @offSet) &#xD;&#xA;set @dtoDate = dbo.Converttoservertime(@toDate, @offSet) &#xD;&#xA;select *  into #tempPayment from(&#xD;&#xA;SELECT mode, &#xD;&#xA;       Sum(void_amount)             void_amount, &#xD;&#xA;       Sum(deposit)                 Deposit, &#xD;&#xA;       Sum(net_amount)              tax_sales, &#xD;&#xA;       Sum(tax)                     tax, &#xD;&#xA;       Sum(gross_total)             Gross_Sales, &#xD;&#xA;       Sum(round_off)               round_off, &#xD;&#xA;       Sum(redeem)                  Redeem, &#xD;&#xA;       Sum(advance)                 Advance, &#xD;&#xA;       Sum(gross_total) + Sum(round_off) &#xD;&#xA;       + Sum(advance) - Sum(redeem) Collection  &#xD;&#xA;FROM   (SELECT TOP 200 CASE &#xD;&#xA;                         WHEN isroundoff = 'Y' THEN cashmode &#xD;&#xA;                         ELSE paymentmode &#xD;&#xA;                       END           mode, &#xD;&#xA;                       Sum(CASE isdebitcard &#xD;&#xA;                             WHEN 'Y' THEN tp.amount &#xD;&#xA;                             ELSE 0 &#xD;&#xA;                           END)      void_amount, &#xD;&#xA;                       Sum(CASE &#xD;&#xA;                             WHEN isdebitcard = 'N' &#xD;&#xA;                                  THEN &#xD;&#xA;                             Isnull(trxv.deposit_amount* (case when tp.amount =0 then 1 else (tp.amount/(case when total_amount = 0 then tp.amount else total_amount end )) end), 0) &#xD;&#xA;                             + Isnull(trxrv.deposit_amount*(case when tp.amount =0 then 1 else (tp.amount/(case when total_amount = 0 then tp.amount else total_amount end )) end), 0) &#xD;&#xA;                             ELSE 0 &#xD;&#xA;                           END)      Deposit, &#xD;&#xA;                       Sum(CASE isdebitcard &#xD;&#xA;                             WHEN 'N' THEN tp.amount - &#xD;&#xA;                                           Isnull(trxv.deposit_amount*( case when tp.amount =0 then 1 else (tp.amount/(case when total_amount = 0 then tp.amount else total_amount end )) end), 0) - &#xD;&#xA;                                           Isnull(trxrv.deposit_amount*(case when tp.amount =0 then 1 else (tp.amount/(case when total_amount = 0 then tp.amount else total_amount end )) end), 0) - &#xD;&#xA;                                                           taxamount * ( 1 - &#xD;&#xA;        Isnull(v.trxdiscountpercentage, 0) / 100.0 ) &#xD;&#xA;        * &#xD;&#xA;        tp.amount / CASE &#xD;&#xA;        trxnetamount &#xD;&#xA;        WHEN 0 THEN NULL &#xD;&#xA;        ELSE trxnetamount &#xD;&#xA;        END &#xD;&#xA;        ELSE 0 &#xD;&#xA;        END)      net_amount, &#xD;&#xA;        Sum(taxamount * ( 1 - Isnull(v.trxdiscountpercentage, 0) / 100.0 ) * &#xD;&#xA;            tp.amount &#xD;&#xA;            / &#xD;&#xA;        CASE trxnetamount &#xD;&#xA;        WHEN 0 THEN NULL &#xD;&#xA;        ELSE trxnetamount &#xD;&#xA;        END)      tax, &#xD;&#xA;        Sum(CASE &#xD;&#xA;        WHEN ( isroundoff = 'Y' &#xD;&#xA;        AND tp.amount BETWEEN -1 AND 1 ) THEN tp.amount &#xD;&#xA;        ELSE 0 &#xD;&#xA;        END) * -1 Round_Off, &#xD;&#xA;        Sum(CASE isdebitcard &#xD;&#xA;        WHEN 'N' THEN tp.amount &#xD;&#xA;        ELSE 0 &#xD;&#xA;        END)      gross_total, &#xD;&#xA;        CASE &#xD;&#xA;        WHEN iscash = 'Y' THEN 'Y' &#xD;&#xA;        ELSE &#xD;&#xA;        CASE &#xD;&#xA;        WHEN isroundoff = 'Y' THEN 'Y' &#xD;&#xA;        ELSE 'N' &#xD;&#xA;        END &#xD;&#xA;        END           isCash, &#xD;&#xA;        isdebitcard, &#xD;&#xA;        Sum(CASE &#xD;&#xA;        WHEN CONVERT(DATE, tp.paymentdate) &lt; &#xD;&#xA;        @dfromDate&#xD;&#xA;        AND CONVERT(DATE, tp.paymentdate) &lt; CONVERT(DATE, v.trxdate) THEN &#xD;&#xA;        tp.amount &#xD;&#xA;        ELSE 0 &#xD;&#xA;        END)      Redeem, &#xD;&#xA;        0             Advance &#xD;&#xA;        FROM   trx_header v &#xD;&#xA;               LEFT OUTER JOIN (SELECT trxid, &#xD;&#xA;                                       Sum(( net_amount * ( &#xD;&#xA;                                             cashratio + creditcardratio &#xD;&#xA;                                                          ) )) &#xD;&#xA;                                                           deposit_amount &#xD;&#xA;                                FROM   transactionview v &#xD;&#xA;                                WHERE  trxdate &gt;= &#xD;&#xA;@dfromDate&#xD;&#xA;AND trxdate &lt; &#xD;&#xA;    @dtoDate&#xD;&#xA;AND ( ( v.site_id IN ( @site ) ) &#xD;&#xA;       OR ( -1 IN ( @site ) ) ) &#xD;&#xA;AND ( v.user_id IN ( @userId ) &#xD;&#xA;       OR -1 IN ( @userId ) ) &#xD;&#xA;AND ( pos_machine IN ( @pos ) ) &#xD;&#xA;AND product_type IS NOT NULL &#xD;&#xA;AND product_type != 'LOYALTY' &#xD;&#xA;AND product_type = 'CARDDEPOSIT' &#xD;&#xA;GROUP  BY trxid) trxv &#xD;&#xA;ON trxv.trxid = v.trxid &#xD;&#xA;LEFT OUTER JOIN (SELECT &#xD;&#xA;             trxid, &#xD;&#xA;Sum(( net_amount * ( cashratio + creditcardratio &#xD;&#xA;                   ) )) &#xD;&#xA;             deposit_amount &#xD;&#xA;FROM   transactionview v &#xD;&#xA;WHERE  trxdate &gt;= &#xD;&#xA;@dfromDate&#xD;&#xA;AND trxdate &lt; &#xD;&#xA;   @dtoDate&#xD;&#xA;AND ( ( v.site_id IN ( @site ) ) &#xD;&#xA;       OR ( -1 IN ( @site ) ) ) &#xD;&#xA;AND ( v.user_id IN ( @userId ) &#xD;&#xA;       OR -1 IN ( @userId ) ) &#xD;&#xA;AND ( pos_machine IN ( @pos ) ) &#xD;&#xA;AND product_type IS NOT NULL &#xD;&#xA;AND product_type != 'LOYALTY' &#xD;&#xA;AND product_type = 'REFUNDCARDDEPOSIT' &#xD;&#xA;GROUP  BY trxid) trxrv &#xD;&#xA;ON trxrv.trxid = v.trxid, &#xD;&#xA;trxpayments tp, &#xD;&#xA;(select trxid,sum(amount) total_amount from trxpayments group by trxid ) tp1, &#xD;&#xA;paymentmodes p, &#xD;&#xA;(SELECT TOP 1 paymentmode cashMode &#xD;&#xA;FROM   paymentmodes &#xD;&#xA;WHERE  iscash = 'Y') cashPmtMode &#xD;&#xA;WHERE  trxdate &gt;= @dfromDate&#xD;&#xA;AND trxdate &lt; @dtoDate&#xD;&#xA;AND v.trxid = tp.trxid &#xD;&#xA;and tp1.trxid = tp.trxid &#xD;&#xA;AND v.status != 'CANCELLED' &#xD;&#xA;AND tp.paymentmodeid = p.paymentmodeid &#xD;&#xA;AND ( ( v.site_id IN ( @site ) ) &#xD;&#xA;OR ( -1 IN ( @site ) ) ) &#xD;&#xA;AND ( v.user_id IN ( @userId ) &#xD;&#xA;OR -1 IN ( @userId ) ) &#xD;&#xA;AND ( pos_machine IN ( @pos ) ) &#xD;&#xA;GROUP  BY paymentmode, &#xD;&#xA;iscash, &#xD;&#xA;iscreditcard, &#xD;&#xA;isdebitcard, &#xD;&#xA;isroundoff, &#xD;&#xA;cashmode &#xD;&#xA;ORDER  BY iscash DESC, &#xD;&#xA;isdebitcard &#xD;&#xA;UNION ALL &#xD;&#xA;SELECT CASE &#xD;&#xA;WHEN isroundoff = 'Y' THEN cashmode &#xD;&#xA;ELSE paymentmode &#xD;&#xA;END      mode, &#xD;&#xA;Sum(CASE isdebitcard &#xD;&#xA;WHEN 'Y' THEN tp.amount &#xD;&#xA;ELSE 0 &#xD;&#xA;END) void_amount, &#xD;&#xA;0, &#xD;&#xA;0, &#xD;&#xA;0, &#xD;&#xA;0, &#xD;&#xA;0, &#xD;&#xA;CASE &#xD;&#xA;WHEN iscash = 'Y' THEN 'Y' &#xD;&#xA;ELSE &#xD;&#xA;CASE &#xD;&#xA;WHEN isroundoff = 'Y' THEN 'Y' &#xD;&#xA;ELSE 'N' &#xD;&#xA;END &#xD;&#xA;END      isCash, &#xD;&#xA;isdebitcard, &#xD;&#xA;0        Redeem, &#xD;&#xA;Sum(CASE &#xD;&#xA;WHEN th.trxdate &gt; @dtoDate THEN &#xD;&#xA;tp.amount &#xD;&#xA;ELSE 0 &#xD;&#xA;END) Advance &#xD;&#xA;FROM   trx_header th, &#xD;&#xA;trxpayments tp, &#xD;&#xA;paymentmodes p, &#xD;&#xA;(SELECT TOP 1 paymentmode cashMode &#xD;&#xA;FROM   paymentmodes &#xD;&#xA;WHERE  iscash = 'Y') cashPmtMode &#xD;&#xA;WHERE  th.trxid = tp.trxid &#xD;&#xA;AND tp.paymentmodeid = p.paymentmodeid &#xD;&#xA;AND tp.paymentdate &gt;= @dfromDate&#xD;&#xA;AND tp.paymentdate &lt; @dtoDate &#xD;&#xA;AND NOT ( trxdate &gt;= @dfromDate&#xD;&#xA;AND trxdate &lt; @dtoDate)&#xD;&#xA;AND ( ( th.site_id IN ( @site ) ) &#xD;&#xA;       OR ( -1 IN ( @site ) ) )  &#xD;&#xA;AND ( th.user_id IN ( @userId ) &#xD;&#xA;OR -1 IN ( @userId ) ) &#xD;&#xA;AND ( pos_machine IN (@pos))&#xD;&#xA;GROUP  BY paymentmode, &#xD;&#xA;iscash, &#xD;&#xA;iscreditcard, &#xD;&#xA;isdebitcard, &#xD;&#xA;isroundoff, &#xD;&#xA;cashmode) v &#xD;&#xA;GROUP  BY mode, &#xD;&#xA;          iscash, &#xD;&#xA;          isdebitcard &#xD;&#xA;  union all&#xD;&#xA;&#xD;&#xA;SELECT 'Gratuity' ,isnull(SUM(GratuityAmount*(GameCardRatio)),0),0,isnull(SUM(GratuityAmount*(cashratio+creditcardratio))*-1,0) ,0,isnull(SUM(GratuityAmount*(cashratio+creditcardratio))*-1,0),0 ,0,0,&#xD;&#xA;isnull(SUM(GratuityAmount*(cashratio+creditcardratio))*-1,0)  FROM transactionview v,site s&#xD;&#xA;  WHERE  v.trxdate between @dfromdate&#xD;&#xA; and @dtodate&#xD;&#xA; and (v.site_id = s.site_id or v.site_id is null)&#xD;&#xA; and (v.site_id in (@Site) or -1 in (@Site))&#xD;&#xA; and v.status NOT IN ('OPEN','RESERVED','BOOKING','pending','CANCELLED','Initiated','Ordered','Delivered')&#xD;&#xA;AND product_type  in('GRATUITY')&#xD;&#xA;AND ( v.user_id IN ( @userId ) OR -1 IN ( @userId ) ) &#xD;&#xA;AND ( pos_machine IN ( @pos ) ) &#xD;&#xA;&#xD;&#xA;) A&#xD;&#xA;&#xD;&#xA;select * from  #tempPayment&#xD;&#xA;union all&#xD;&#xA;select 'Total',sum(void_amount),sum(Deposit),sum(tax_sales),sum(tax),sum(Gross_Sales),sum(round_off),sum(Redeem),sum(Advance),sum(Collection) from #tempPayment&#xD;&#xA;&#xD;&#xA; union all&#xD;&#xA;&#xD;&#xA;SELECT 'ServiceCharges' ,isnull(SUM((ServiceChargesAmount+isnull(taxonservicecharges,0))*(GameCardRatio))*-1,0),0,isnull(SUM((ServiceChargesAmount)*(cashratio+creditcardratio))*-1 ,0),isnull(sum(isnull(taxonservicecharges,0)*(cashratio+creditcardratio))*-1,0),&#xD;&#xA;isnull(SUM((ServiceChargesAmount+isnull(taxonservicecharges,0))*(cashratio+creditcardratio))*-1,0),0 ,0,0,isnull(SUM((ServiceChargesAmount+isnull(taxonservicecharges,0))*(cashratio+creditcardratio))*-1,0)  FROM transactionview v,site s&#xD;&#xA;  WHERE  v.trxdate between @dfromdate&#xD;&#xA; and @dtodate&#xD;&#xA; and (v.site_id = s.site_id or v.site_id is null)&#xD;&#xA; and (v.site_id in (@Site) or -1 in (@Site))&#xD;&#xA; and v.status NOT IN ('OPEN','RESERVED','BOOKING','pending','CANCELLED','Initiated','Ordered','Delivered')&#xD;&#xA;AND product_type  in('SERVICECHARGE')&#xD;&#xA;AND ( v.user_id IN ( @userId ) OR -1 IN ( @userId ) ) &#xD;&#xA;AND ( pos_machine IN ( @pos ) ) &#xD;&#xA;&#xD;&#xA;&#xD;&#xA;--ORDER  BY iscash DESC,           isdebitcard &#xD;&#xA;" CommandTimeout="600" Name="dsPaymentModeBreakdown">
