    <SqlDataSource QueryDesignerState="null" ConnectionString="Parafait" SelectCommand="&#xD;&#xA;&#xD;&#xA;SELECT b.BookingProductId,&#xD;&#xA;                                                                  tl.product_id,&#xD;&#xA;                                                                  p.product_name,&#xD;&#xA;                                                                  CAST(tl.Quantity AS INT) Quantity,&#xD;&#xA;                                                                  tl.Remarks &#xD;&#xA;  ,p.price&#xD;&#xA;                                                           FROM &#xD;&#xA;                                                                Bookings b&#xD;&#xA;                                                           INNER JOIN&#xD;&#xA;                                                             (SELECT TRXID,&#xD;&#xA;                                                                     PRODUCT_ID,&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; ComboProductId,&#xD;&#xA;                                                                     remarks,&#xD;&#xA;                                                                     SUM(TL.QUANTITY) Quantity&#xD;&#xA;                                                              FROM trx_lines tl&#xD;&#xA;                                                              WHERE tl.ParentLineId IS NOT NULL &#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   and tl.CancelledTime is null&#xD;&#xA;                                                              GROUP BY TRXID,&#xD;&#xA;                                                                       PRODUCT_ID,&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ComboProductId,&#xD;&#xA;                                                                       Remarks) tl ON b.TrxId = tl.TrxId&#xD;&#xA;                                                           INNER JOIN products p ON p.product_id = tl.product_id&#xD;&#xA;                                                           INNER JOIN (select distinct cb.id, cb.AdditionalProduct, cb.Product_Id, cb.ChildProductId, pp.product_Id as bookingChildProductId&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;                 FROM ComboProduct cb ,&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;      Products pp&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;    WHERE (cb.ChildProductId = pp.product_id ) ) as cbp&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ON cbp.bookingChildProductId = tl.product_id and cbp.id = tl.ComboProductId&#xD;&#xA;                                                                    AND b.BookingId = @BookingId&#xD;&#xA;                                                           WHERE ISNULL(cbp.AdditionalProduct,'N') ='N'&#xD;&#xA;                                                             AND b.BookingId = @BookingId&#xD;&#xA;                                                             AND cbp.Product_Id = b.BookingProductId&#xD;&#xA;" Name="dsPackageproducts">
