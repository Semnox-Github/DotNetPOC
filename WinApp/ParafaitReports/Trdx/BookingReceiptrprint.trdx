Id = tl.TrxId&#xD;&#xA;                                                           INNER JOIN products p ON p.product_id = tl.product_id&#xD;&#xA;                                                            INNER JOIN (select distinct cb.id, cb.AdditionalProduct, cb.Product_Id, cb.ChildProductId, pp.product_Id as bookingChildProductId&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;                 FROM ComboProduct cb ,&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;      Products pp&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;    WHERE (cb.ChildProductId = pp.product_id )) as cbp&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ON cbp.bookingChildProductId = tl.product_id and cbp.id = tl.ComboProductId&#xD;&#xA;                                                           AND b.BookingId = @BookingId&#xD;&#xA;                                                           WHERE ISNULL(cbp.AdditionalProduct,'N') ='Y'&#xD;&#xA;                                                             AND b.BookingId = @BookingId&#xD;&#xA;                                                             AND cbp.Product_Id = b.BookingProductId" Name="dsAddictionalProducts">
