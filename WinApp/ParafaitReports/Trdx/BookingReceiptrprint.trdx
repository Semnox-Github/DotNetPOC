    <SqlDataSource QueryDesignerState="null" ConnectionString="ParafaitGK" SelectCommand="&#xD;&#xA;set @serv= (select count(*) from TransactionView tv,Bookings b &#xD;&#xA;where tv.product_type in ('SERVICECHARGE','GRATUITY') and tv.trxid=b.trxid and b.bookingid=@bookingid)&#xD;&#xA;&#xD;&#xA;if @serv=0&#xD;&#xA;begin&#xD;&#xA;&#xD;&#xA;select  ' ' product_name,0 Amount&#xD;&#xA;end&#xD;&#xA;else&#xD;&#xA;begin&#xD;&#xA;&#xD;&#xA;select isnull(tv.product_name,' ') product_name,(Case when(tv.product_type='SERVICECHARGE') then isnull((tv.ServiceChargesAmount+isnull(TaxOnServiceCharges,0)),0) else isnull(tv.GratuityAmount,0) end) Amount from TransactionView tv,Bookings b &#xD;&#xA;where tv.product_type in ('SERVICECHARGE','GRATUITY') and tv.trxid=b.trxid and b.bookingid=@bookingid&#xD;&#xA;end" Name="dsServiceAndGratuity">
