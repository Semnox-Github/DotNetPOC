            <String>= Parameters.cardNumber.Value</String>
          </Value>
        </SqlDataSourceParameter>
        <SqlDataSourceParameter DbType="Int32" Name="@site">
          <Value>
            <String>=Parameters.site.Value</String>
          </Value>
        </SqlDataSourceParameter>
      </Parameters>
    </SqlDataSource>
    <SqlDataSource QueryDesignerState="null" ConnectionString="parafaitOld" SelectCommand="SELECT  card_number + case isnull(isnull(cu.customer_name + isnull(cu.last_name, ''), c.notes), '') when '' then '' else ' - ' + isnull(cu.customer_name + isnull(cu.last_name, ''), c.notes) end card_number_label, card_number&#xD;&#xA;FROM cards c left outer join customers cu on cu.customer_id = c.customer_id  &#xD;&#xA;WHERE valid_flag = 'Y' &#xD;&#xA;AND technician_card = 'Y'&#xD;&#xA;AND ((c.site_id in (@site)) or (-1 in (@site)))&#xD;&#xA;order by 2, 1&#xD;&#xA;" CommandTimeout="600" Name="dsCardNumber">
