  <DataSources>
    <SqlDataSource QueryDesignerState="null" ConnectionString="Parafait" SelectCommand="select site_name Site, c.card_number + case isnull(isnull(cu.customer_name + isnull(cu.last_name, ''), c.notes), '') when '' then '' else ' - ' + isnull(cu.customer_name + isnull(cu.last_name, ''), c.notes) end card_number, &#xD;&#xA;            dateadd(MI,@offset*-1,play_date) play_date, &#xD;&#xA;            substring(machine_name, 1, 25) as game_name, machine_address as ID, &#xD;&#xA;            SUM(SIGN(v.credits)) AS credit_play_count, &#xD;&#xA;            SUM(SIGN(v.courtesy)) AS crtsy_play_count, &#xD;&#xA;            SUM(SIGN(v.bonus)) AS bonus_play_count, &#xD;&#xA;            SUM(SIGN(v.time)) AS time_play_count, &#xD;&#xA;            COUNT(v.credits) AS total_plays, &#xD;&#xA;            SUM(v.credits) AS credits, &#xD;&#xA;            SUM(v.courtesy) AS courtesy, &#xD;&#xA;            SUM(v.bonus) AS bonus, &#xD;&#xA;            SUM(v.time) AS time, &#xD;&#xA;            SUM(v.credits + v.courtesy + v.bonus + v.time) AS total_amount, &#xD;&#xA;            SUM(v.ticket_count) AS tickets &#xD;&#xA; from GameMetricView v inner join site s on (v.site_id = s.site_id or v.site_id is null) , cards c &#xD;&#xA;&#x9;&#x9;left outer join customers cu &#xD;&#xA;        on cu.customer_id = c.customer_id &#xD;&#xA;where play_date &gt;= dbo.ConvertToServerTime(@fromDate,@offSet)&#xD;&#xA;and play_date &lt; dbo.ConvertToServerTime(@toDate,@offSet) &#xD;&#xA;and c.card_id = v.card_id  and c.valid_flag='y' &#xD;&#xA;and c.technician_card = 'Y' &#xD;&#xA;and ((c.card_id in (@cardNumber)) or (-1 in (@cardNumber)))&#xD;&#xA;AND ((v.site_id in (@site)) or (-1 in (@site)))&#xD;&#xA;group by site_name, c.card_number + case isnull(isnull(cu.customer_name + isnull(cu.last_name, ''), c.notes), '') when '' then '' else ' - ' + isnull(cu.customer_name + isnull(cu.last_name, ''), c.notes) end, &#xD;&#xA;    play_date, game_profile_name, substring(machine_name, 1, 25), machine_address, machine_id &#xD;&#xA;order by 3, 4" CommandTimeout="600" Name="dsTechnicianCardReport">
